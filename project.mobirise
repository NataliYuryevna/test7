        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-wrapper, .card-wrapper .card-box, .card-wrapper .card-img": {
              "position": "relative"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor",
              ".mbr-overlay": {
                "background": "transparent",
                "opacity": "1"
              }
            },
            ".card-wrapper .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".card-wrapper when (@showMarkers)": {
              "flex-direction": "row",
              ".card-img": {
                "width": "auto",
                ".amp-iconfont": {
                  "font-size": "~\"@{markersSize}rem\"",
                  "color": "@markersColor !important",
                  "@media (max-width: 767px)": {
                    "font-size": "50px"
                  }
                },
                ".card-marker": {
                  "display": "inline-block",
                  "width": "~\"@{markersSize}rem\"",
                  "height": "~\"@{markersSize}rem\"",
                  "background-color": "@markersColor !important",
                  "border-radius": "2px",
                  "@media (max-width: 767px)": {
                    "max-width": "30px",
                    "max-height": "30px"
                  }
                }
              },
              ".card-box": {
                "padding-left": "15px"
              }
            },
            ".card-wrapper when not (@transparentBg), (@showBorders), (@showCardsEffect)": {
              "& when not (@transparentBg), (@showBorders)": {
                "padding": "40px 30px"
              },
              "@media (max-width: 991px)": {
                "padding": "15px"
              }
            },
            ".card-title when (@showLine)": {
              "display": "inline-block",
              "&:after": {
                "content": "\"\"",
                "display": "block",
                "background-color": "@primaryColor",
                "height": "3px",
                "width": "25px",
                "& when (@cardsAlign = \"2\")": {
                  "margin": "0 auto"
                },
                "& when (@cardsAlign = \"3\")": {
                  "margin-left": "auto"
                },
                "margin-top": "15px",
                "transition": "all .2s ease"
              }
            },
            ".card-wrapper:hover .card-title:after when (@showLineEffect) and (@showLine)": {
              "width": "100%"
            },
            "& when (@showCardsEffect)": {
              ".card-wrapper when not (@showBorders) and (@transparentBg)": {
                "&, .card-img .amp-iconfont, .card-box .card-title": {
                  "transition": "all 0.2s linear"
                },
                "&:hover": {
                  "box-shadow": "0px 0px 40px 1px rgba(0, 0, 0, 0.1)",
                  ".card-img .amp-iconfont, .card-box .card-title": {
                    "color": "@primaryColor !important"
                  }
                }
              },
              ".card-wrapper when (@showBorders), not (@transparentBg)": {
                ".mbr-overlay, .card-img .amp-iconfont, .card-box, .card-box .btn": {
                  "transition": "all 0.2s linear"
                },
                "&:hover": {
                  ".mbr-overlay": {
                    "background": "@primaryColor"
                  },
                  ".card-img .amp-iconfont, .card-box, .card-box .btn[class*=\"-outline\"], .card-box .btn[class*=\"-outline\"]:hover": {
                    "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                  },
                  ".card-box .card-title:after, .card-img .card-marker": {
                    "background-color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                  }
                }
              }
            },
            "@media (max-width: 991px)": {
              ".card.md-pb": {
                "padding-bottom": "50px"
              }
            },
            "@media (max-width: 767px)": {
              ".card:not(.last-child)": {
                "padding-bottom": "20px"
              }
            },
            ".mbr-row when (@showBorders) and (@showBorderTop) and not (@showPadding)": {
              "border-top": "1px solid @bordersColor"
            },
            ".mbr-row .card when not (@showPadding)": {
              "padding": "0",
              "& when (@showBorders)": {
                ".card-wrapper": {
                  "border": "none"
                },
                "@media (min-width: 768px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "1px solid @bordersColor"
                  }
                },
                "@media (max-width: 991px)": {
                  "&.md-pb .card-wrapper": {
                    "border-bottom": "1px solid @bordersColor"
                  },
                  "&.md-pb:nth-child(2) .card-wrapper": {
                    "border-right": "none"
                  }
                },
                "@media (max-width: 767px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "none",
                    "border-bottom": "1px solid @bordersColor"
                  }
                }
              }
            }
