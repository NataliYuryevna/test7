          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".text-content": {
              "overflow": "hidden"
            },
            ".card-box, .card-container, .carousel-img, .dots-wrapper, .container-img.img-with-overlay:hover amp-img:before": {
              "width": "100%"
            },
            ".card-container, .card-wrap, .image-wrap, .slider-box, .container-img.img-with-overlay:hover amp-img": {
              "position": "relative"
            },
            "& when (@showPadding)": {
              ".card-container": {
                "@media (min-width: 992px)": {
                  ".card-wrap": {
                    "padding-right": "70px"
                  },
                  "&.mbr-row-reverse .card-wrap": {
                    "padding-left": "70px",
                    "padding-right": "15px"
                  }
                }
              },
              "@media (min-width: 1200px)": {
                ".container-fluid .mbr-row .card-wrap": {
                  "padding-left": "100px",
                  "padding-right": "100px"
                }
              }
            },
            ".card-container when not (@transparentBg)": {
              ".mbr-overlay": {
                "background": "@cardBg",
                "opacity": "@cardOpacity"
              },
              ".image-wrap": {
                "margin": "0"
              },
              ".image-wrap, .carousel-img.mbr-col-md-12": {
                "padding": "0"
              }
            },
            ".card-marker:before": {
              "display": "inline-block",
              "vertical-align": "middle"
            },
            ".mbr-list when (@stylizedCounters = \"1\")": {
              "counter-reset": "decimal",
              ".card-marker:before": {
                "counter-increment": "decimal",
                "content": "\"0\" counter(decimal)",
                "color": "@primaryColor",
                "font-size": "~\"@{widthImg}rem\"",
                "& when (@countersBorder)": {
                  "border-bottom": "2px solid @primaryColor"
                }
              }
            },
            ".card-marker:before when (@stylizedCounters = \"2\")": {
              "content": "\"\"",
              "border": "3px solid @primaryColor",
              "width": "~\"calc(@{widthImg}rem - 6px)\"",
              "height": "~\"calc(@{widthImg}rem - 6px)\"",
              "box-sizing": "initial"
            },
            ".mbr-list .mbr-flex": {
              "flex-direction": "row",
              ".card-marker, .card-icon": {
                "margin-right": "15px"
              },
              "&:not(:last-child)": {
                "padding-bottom": "10px"
              }
            },
            ".card-icon .amp-iconfont": {
              "color": "@primaryColor",
              "font-size": "~\"@{widthImg}rem\""
            },
            "amp-carousel": {
              "height": "~\"@{slideHeight}px\" !important"
            },
            ".carousel-img": {
              "height": "~\"calc(@{slideHeight}px - 85px)\"",
              "margin": "0",
              "margin-bottom": "85px",
              ".container-img": {
                "& when not (@showImgTitle) and not (@showImgText)": {
                  "height": "~\"calc(@{slideHeight}px - 85px)\""
                },
                "& when (@showImgTitle), (@showImgText)": {
                  "height": "~\"calc(@{slideHeight}px - 185px)\""
                },
                "amp-img,img": {
                  "object-fit": "cover"
                }
              },
              ".text-wrap": {
                "height": "100px",
                "padding-top": "35px",
                "& when (@lineText) and (@showImgText)": {
                  ".mbr-text:before": {
                    "content": "''",
                    "width": "25px",
                    "height": "3px",
                    "display": "inline-block",
                    "vertical-align": "middle",
                    "margin-right": "10px",
                    "background-color": "@primaryColor"
                  }
                }
              }
            },
            ".amp-carousel-button": {
              "background-color": "@buttonsColor",
              "-webkit-transition": "all 0.3s",
              "transition": "all 0.3s",
              "top": "~\"calc(100% - 27px)\"",
              "opacity": "0.8",
              "&:before, &:after": {
                "color": "contrast(@buttonsColor, darken(@buttonsColor, 50%), #ffffff, 60%) !important"
              },
              "&:hover": {
                "opacity": "1"
              },
              "&.amp-carousel-button-prev": {
                "left": "0"
              },
              "&.amp-carousel-button-next": {
                "left": "55px"
              },
              "@media (min-width: 992px)": {
                "& when not (@countImg = 1)": {
                  "&.amp-carousel-button-prev": {
                    "left": "15px"
                  },
                  "&.amp-carousel-button-next": {
                    "left": "(15px + 55px)"
                  }
                }
              },
              "@media (min-width: 1200px)": {
                "& when (@countImg = 2)": {
                  "&.amp-carousel-button-prev": {
                    "left": "30px"
                  },
                  "&.amp-carousel-button-next": {
                    "left": "(30px + 55px)"
                  }
                }
              }
            },
            "@media (max-width: 991px)": {
              ".card-wrap when (@transparentBg)": {
                "padding-bottom": "50px"
              }
            },
            ".container-img.img-with-overlay:hover amp-img": {
              "&:before": {
                "content": "\"\"",
                "position": "absolute",
                "height": "100%",
                "background-color": "@imgOverlayColor",
                "opacity": "@imgOverlayOpacity",
                "z-index": "1",
                "pointer-events": "none"
              },
              "& > a:before when (@showImgButton)": {
                "content": "\"\"",
                "position": "absolute",
                "top": "50%",
                "left": "50%",
                "font-size": "40px",
                "transform": "translate(-50%, -50%)",
                "color": "contrast(@imgOverlayColor, darken(@imgOverlayColor, 50%), #ffffff, 60%)",
                "z-index": "2"
              }
            }
          },
          "_name": "slider3",
          "_customHTML": "<section class=\"slider3 slider\" group=\"Sliders\" plugins=\"amp-main, amp-carousel\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Width Images\" name=\"columnWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showCardButtons\">    \n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showCardButtons\">\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked=\"\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>    \n    <header>Images</header>\n        <input type=\"range\" inline=\"\" title=\"Slider Height\" name=\"slideHeight\" min=\"300\" max=\"700\" step=\"10\" value=\"700\">\n        <input type=\"range\" inline=\"\" title=\"Count Images Slider\" name=\"countImg\" min=\"1\" max=\"4\" step=\"1\" value=\"2\">\n        <select title=\"Sliders Count\" name=\"slidersAmount\">\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\" selected=\"\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\">8</option>\n        </select>\n        <input type=\"color\" title=\"Buttons Color\" name=\"buttonsColor\" value=\"#273272\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showImgTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showImgText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Line Before Text\" name=\"lineText\" condition=\"showImgText\" checked=\"\">\n        <select title=\"Align\" name=\"textImgAlign\" condition=\"showImgTitle || showImgText\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Image Overlay\" name=\"showImgOverlay\" checked=\"\">\n        <input type=\"color\" title=\"Image Overlay Color\" name=\"imgOverlayColor\" value=\"#000000\" condition=\"showImgOverlay\">\n        <input type=\"range\" inline=\"\" title=\"Image Overlay Opacity\" name=\"imgOverlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"showImgOverlay\">\n        <input type=\"checkbox\" title=\"Show Image Button\" name=\"showImgButton\" condition=\"showImgOverlay\" checked=\"\">\n    <header>Text</header>    \n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showCardSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showCardText\" checked=\"\">\n        <input type=\"checkbox\" title=\"List\" name=\"showCardList\" checked=\"\">\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"listCount\" min=\"1\" max=\"9\" step=\"1\" value=\"3\" condition=\"showCardList\"> \n            <select title=\"Stylized Counters\" name=\"stylizedCounters\" condition=\"showCardList\">\n                <option valu