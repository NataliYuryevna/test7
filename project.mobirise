                }
              },
              ".infNumerically": {
                "& when (@titleAlign = \"2\")": {
                  "@media (min-width: 768px)": {
                    "position": "absolute",
                    "top": "0",
                    "& when (@infCount < 4)": {
                      "& > div:first-child": {
                        "margin-right": "auto"
                      }
                    },
                    "@media (min-width: 991px)": {
                      "& when (@infCount = 4)": {
                        "& > div:nth-child(2)": {
                          "margin-right": "auto"
                        }
                      }
                    }
                  }
                }
              }
            },
            ".mbr-px-0": {
              "padding-left": "0",
              "padding-right": "0"
            },
            ".card-wrapper, .card-wrapper .card-box": {
              "position": "relative"
            },
            ".card-wrapper .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-img.img-with-overlay:hover amp-img": {
              "position": "relative",
              "&:before": {
                "content": "\"\"",
                "position": "absolute",
                "height": "100%",
                "width": "100%",
                "background-color": "@imgOverlayColor",
                "opacity": "@imgOverlayOpacity",
                "z-index": "1",
                "pointer-events": "none"
              },
              "& > a:before when (@showImgButton)": {
                "content": "\"＋\"",
                "position": "absolute",
                "top": "50%",
                "left": "50%",
                "font-size": "40px",
                "transform": "translate(-50%, -50%)",
                "color": "contrast(@imgOverlayColor, darken(@imgOverlayColor, 50%), #ffffff, 60%)",
                "z-index": "2"
              }
            },
            "& when (@showPadding)": {
              "@media (min-width: 768px)": {
                ".card:not(.last-child)": {
                  "padding-bottom": "60px"
                }
              },
              "@media (max-width: 767px)": {
                ".card:not(.last-child)": {
                  "padding-bottom": "40px"
                }
              }
            }
