                "&:not(:first-child) when (@showBorderRight)": {
                  "border-left": "1px solid @borderColor"
                }
              }
            },
            "@media (max-width: 991px)": {
              ".second-element when (@transparentBg)": {
                "padding-bottom": "50px"
              }
            },
            "@media (max-width: 767px)": {
              ".milestone": {
                "text-align": "center",
                ".numeral": {
                  "justify-content": "center"
                },
                "& > div:not(:last-child)": {
                  "margin-bottom": "20px"
                }
              }
            },
            ".gallery-img-wrap:after when not (@showImgOverlay)": {
