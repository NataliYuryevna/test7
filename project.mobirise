          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@fullHeight) and not (@showTitle) and not (@showSubtitle)": {
              "padding": "0",
              ".card-img, .card-img img, .card-img amp-img, .card-box": {
                "min-height": "450px",
                "@media (max-width: 991px)": {
                  "min-height": "350px"
                }
              },
              ".card-box when not (@showImg)": {
                "@media (min-width: 992px)": {
                  "min-height": "900px"
                }
              },
              ".card-box when not (@showImg) and (@transparentBg) and not (@showBorders)": {
                "min-height": "270px"
              }
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-benefits-box": {
              "align-items": "center",
              ".card-number when (@showLineNumber)": {
                "position": "relative",
                "padding-left": "70px",
                "& when not (@transparentBg), (@showBorders)": {
                  "& when not (@fullWidth)": {
                    "padding-left": "30px"
                  }
                },
                "&:before": {
                  "content": "''",
                  "width": "50px",
                  "& when not (@transparentBg), (@showBorders)": {
                    "& when not (@fullWidth)": {
                      "width": "10px"
                    }
                  },
                  "height": "1px",
                  "position": "absolute",
                  "left": "10px",
                  "top": "50%",
                  "-webkit-transform": "translateY(-50%)",
                  "transform": "translateY(-50%)",
                  "background-color": "@lineColor"
                }
              }
            },
            ".card-box": {
              "width": "100%",
              "& when (@showBorders), not (@transparentBg)": {
                "padding": "60px",
                "@media (max-width: 991px)": {
                  "padding": "25px"
                }
              }
            },
            ".card-wrapper, .card-wrapper .card-box": {
              "position": "relative"
            },
            ".card-wrapper .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-wrapper when (@showCardsEffect)": {
              "& when (@showCardsTitleNumber), (@showCardsTitle)": {
                "& when (@showCardsSubtitle), (@showCardsText), (@showCardsButtons)": {
                  "&:hover .card-benefits-box, .card-text-box > :not(.card-title)": {
                    "opacity": "0"
                  },
                  ".card-benefits-box, &:hover .card-text-box > :not(.card-title)": {
                    "opacity": "1"
                  },
                  ".card-text-box when (@showCardsSubtitle)": {
                    "margin-top": "-25px"
                  },
                  ".card-text-box when not (@showCardsSubtitle) and (@showCardsTitleNumber)": {
                    "margin-bottom": "-25px"
                  },
                  "&:hover .card-text-box": {
                    "margin-top": "0",
                    "margin-bottom": "0"
                  },
                  "&:hover .card-benefits-box": {
                    "max-height": "0",
                    "padding": "0"
                  },
                  ".card-benefits-box": {
                    "transition": "all 0.3s linear"
                  },
                  ".card-subtitle, .mbr-text, .card-btn": {
                    "transition": "all 0.2s linear 0.1s"
                  },
                  "&:hover when not (@transparentBg), (@showBorders)": {
                    "& when (@showBorders) and (@transparentBg)": {
                      "background-color": "@primaryColor"
                    },
                    ".mbr-overlay": {
                      "background": "@primaryColor"
                    },
                    ".card-benefits-box h4, .card-text-box > *, .card-box .btn[class*=\"-outline\"], .card-box .btn[class*=\"-outline\"]:hover": {
                      "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                    },
                    ".card-box .btn[class*=\"-outline\"], .card-box .btn[class*=\"-outline\"]:hover": {
                      "border-color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                    }
                  }
                }
              }
            },
            ".mbr-row .card when not (@showPadding)": {
              "padding": "0",
              "& when (@showBorders)": {
                ".card-wrapper": {
                  "border": "none"
                },
                "@media (min-width: 768px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "1px solid @bordersColor"
                  },
                  "@media (max-width: 991px)": {
                    "&.md-pb .card-wrapper": {
                      "border-bottom": "1px solid @bordersColor"
                    },
                    "&.md-pb:nth-child(2) .card-wrapper": {
                      "border-right": "none"
                    }
                  }
                },
                "@media (max-width: 767px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "none",
                    "border-bottom": "1px solid @bordersColor"
                  }
                }
              }
            }
