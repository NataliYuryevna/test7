              ".infNumerically > div:not(:last-child) when (@columnWidth = 5)": {
                "margin-bottom": "20px"
              },
              ".infNumerically when (@infCount > 2) and not (@columnWidth = 5)": {
                "& > div:nth-child(2n+1)": {
                  "border-left": "none"
                },
                "& > div:not(:last-child)": {
                  "margin-bottom": "20px"
                },
                "& > div:nth-last-child(2) when (@infCount = 4)": {
                  "margin-bottom": "0px"
                }
              },
              "& when not (@showBullets)": {
                ".slide": {
                  "height": "~\"calc(@{slideHeight}px - 85px)\"",
                  "margin-bottom": "85px"
                },
                ".amp-carousel-button": {
                  "display": "block",
                  "top": "~\"calc(100% - 27px)\"",
                  "opacity": "0.8",
                  "&:after": {
                    "border-color": "contrast(@buttonsColor, darken(@buttonsColor, 50%), #ffffff, 60%) !important"
                  },
                  "&:hover": {
                    "opacity": "1"
                  }
                },
                ".amp-carousel-button-prev": {
                  "left": "0"
                },
                ".amp-carousel-button-next": {
                  "left": "55px"
                },
                ".dots-wrapper": {
                  "display": "none"
                }
              }
            },
            ".card-box .infNumerically > div when (@infCount = 3) and (@fullWidth) and not (@columnWidth = 5)": {
              "@media (min-width: 1200px)": {
                "margin-bottom": "0px",
                "&:not(:first-child) when (@infBorderRight)": {
                  "border-left": "1px solid @borderColor"
                }
              }
            },
            "@media (max-width: 767px)": {
              ".container-img when (@showImgText), (@showImgTitle)": {
                "height": "50%"
              },
              ".text-wrap when (@showImg)": {
                "height": "50%"
              },
              ".card-wrap when (@transparentBg)": {
                "& when (@showImgText), (@showImgTitle), (@showImg)": {
                  "padding-bottom": "50px"
                }
              },
              ".text-wrap when (@showImg) and (@testimonialsBg)": {
                "padding-top": "25px"
              },
              ".infNumerically": {
                "text-align": "center",
                ".numeral": {
                  "justify-content": "center"
                },
                "& > div:not(:last-child)": {
                  "margin-bottom": "20px"
                }
              }
            }
          },
          "_name": "slider4",
          "_customHTML": "<section class=\"slider4 slider slider-with-bullets\" group=\"Sliders\" plugins=\"amp-main, amp-carousel, amp-bind\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline=\"\" title=\"Width Images\" name=\"columnWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"(showCardTitle || showCardSubtitle || showCardList || showCardText || showInfNumerically) &amp;&amp; (showImgText || showImgTitle || showImg)\">    \n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"(showCardTitle || showCardSubtitle || showCardList || showCardText || showInfNumerically) &amp;&amp; (showImgText || showImgTitle || showImg)\">\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked=\"\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>    \n    <header>Testimonials</header>\n        <input type=\"range\" inline=\"\" title=\"Width Testimonials\" name=\"testimonialsWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"(showImgText || showImgTitle) &amp;&amp; showImg\">\n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"testimonialsReverseContent\" condition=\"(showImgText || showImgTitle) &amp;&amp; showImg\">\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\" checked=\"\" condition=\"slidersAmount > 1 &amp;&amp; (showImgText || showImgTitle || showImg)\">\n            <input type=\"range\" inline=\"\" title=\"Delay\" name=\"delay\" min=\"1000\" max=\"5000\" step=\"1000\" value=\"3000\" condition=\"autoplay &amp;&amp; slidersAmount > 1 &amp;&amp; (showImgText || showImgTitle || showImg)\">\n        <input type=\"range\" inline=\"\" title=\"Slider Height\" name=\"slideHeight\" min=\"300\" max=\"700\" step=\"10\" value=\"480\" condition=\"showImgText || showImgTitle || showImg\">\n        <select title=\"Sliders Count\" name=\"slidersAmount\" condition=\"showImgText || showImgTitle || showImg\">\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\" selected=\"\">8</option>\n        </select>\n        <input type=\"checkbox\" title=\"Show Buttons/Bullets\" name=\"showBullets\" condition=\"showImgText || showImgTitle || showImg\">\n            <input type=\"color\" title=\"Color\" name=\"buttonsColor\" value=\"#273272\" condition=\"showImgText || showImgTitle || showImg\">\n        <input type=\"checkbox\" title=\"Image\" name=\"showImg\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showImgText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showImgTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Line Before Title\" name=\"lineTitle\" condition=\"showImgTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"testimonialsBg\" condition=\"showImgText || showImgTitle\">\n        <input type=\"color\" title=\"Color\" name=\"textBg\" value=\"#ffffff\" condition=\"!testimonialsBg &amp;&amp; (showImgText || showImgTitle)\">\n        <select title=\"Align\" name=\"textImgAlign\" condition=\"showImgTitle || showImgText\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n    <header>Text</header>    \n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showCardSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showCardText\" checked=\"\">\n        <input type=\"checkbox\" title=\"List\" name=\"showCardList\" checked=\"\">\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"listCount\" min=\"1\" max=\"9\" step=\"1\" value=\"2\" condition=\"showCardList\"> \n            <select title=\"Stylized Counters\" name=\"stylizedCounters\" condition=\"showCardList\">\n                <option value=\"1\" selected=\"\">Counters</option>\n                <option value=\"2\">Markers</option>\n                <option value=\"3\">Icons</option>\n            </select>\n            <input type=\"checkbox\" title=\"Border Bottom Counters\" name=\"countersBorder\" checked=\"\" condition=\"showCardList &amp;&amp; stylizedCounters == 1\">\n            <input type=\"range\" inline=\"\" title=\"Width Marker\" name=\"widthImg\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"0.8\" condition=\"showCardList\">\n            <input type=\"checkbox\" title=\"Title List\" name=\"showCardListTitle\" condition=\"showCardList\">\n            <input type=\"checkbox\" title=\"Text List\" name=\"showCardListText\" checked=\"\" condition=\"showCardList\">\n        <input type=\"checkbox\" title=\"Information Numerically\" name=\"showInfNumerically\" checked=\"\">\n            <input type=\"checkbox\" title=\"Border Top\" name=\"infBorderTop\" condition=\"showInfNumerically\" checked=\"\">\n            <input type=\"checkbox\" title=\"Border Right\" name=\"infBorderRight\" condition=\"showInfNumerically &amp;&amp; infCount != 1 &amp;&amp; columnWidth != 5\" checked=\"\">\n            <input type=\"color\" title=\"Color Border\" name=\"borderColor\" value=\"#d4d6e3\" condition=\"showInfNumerically &amp;&amp; (infBorderTop || (infBorderRight &amp;&amp; infCount != 1))\">\n            <input type=\"range\" inline=\"\" title=\"Count Information\" name=\"infCount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\" condition=\"showInfNumerically\">\n        <select title=\"Align\" name=\"cardAlign\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showInfNumerically\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Padding\" name=\"showPadding\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showInfNumerically\" checked=\"\">\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardTitle || showCardSubtitle || showCardText || showCardList || showInfNumerically\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#fac769\" condition=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardText || showCardList || showInfNumerically)\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardText || showCardList || showInfNumerically)\">\n    <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\" checked=\"\"> \n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background1.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#efefef\" selected=\"\">\n        </fieldset>\n        <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#ffffff\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"title-wrap mbr-pb-4\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-left': titleAlign==1,\n                         'align-center': titleAlign==2,\n                         'align-right': titleAlign==3, 'mbr-px-2': fullWidth}\">\n            <h1 class=\"mbr-section-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">\n                Slider</h1>\n            <h2 class=\"mbr-section-subtitle mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">Lorem ipsum dolor sit amet, consectetur adipisicing elit.</h2>\n        </div> \n        <div class=\"mbr-row\" mbr-class=\"{'mbr-px-2': !transparentBg}\">\n            <div class=\"card-container mbr-flex flex-wrap mbr-jc-c\" mbr-class=\"{'mbr-row-reverse':reverseContent &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showInfNumerically)}\">\n                <div class=\"mbr-overlay\" mbr-if=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showInfNumerically)\"></div>\n                <div class=\"card-wrap mbr-flex mbr-col-sm-12\" mbr-if=\"showCardTitle || showCardSubtitle || showCardText || showInfNumerically || showCardList\" mbr-class=\"{'mbr-col-md-8':columnWidth==1 &amp;&amp; (showImgText || showImgTitle || showImg),\n                                     'mbr-col-md-7':columnWidth==2 &amp;&amp; (showImgText || showImgTitle || showImg),\n                                     'mbr-col-md-6':columnWidth==3 &amp;&amp; (showImgText || showImgTitle || showImg),\n                                     'mbr-col-md-5':columnWidth==4 &amp;&amp; (showImgText || showImgTitle || showImg),\n                                     'mbr-col-md-4':columnWidth==5 &amp;&amp; (showImgText || showImgTitle || showImg)}\">\n                        <div class=\"card-box mbr-m-auto\" mbr-class=\"{'align-center': cardAlign==2,\n                                     'align-right': cardAlign==3, 'mbr-py-4': !transparentBg, 'mbr-px-2': transparentBg &amp;&amp; fullWidth}\">\n                            <h2 class=\"card-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\" data-app-selector=\".card-title\">About Our</h2>\n                            <h3 class=\"card-subtitle mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showCardSubtitle\" data-app-selector=\".card-subtitle\">Company</h3>\n                            <div class=\"mbr-list\" mbr-list=\"\" mbr-list-grow=\"listCount\" mbr-if=\"showCardList\" mbr-class=\"{'mbr-pt-4': showCardTitle || showCardSubtitle}\">\n                                <div class=\"mbr-flex\">\n                                    <div class=\"card-marker\" mbr-if=\"stylizedCounters == 1 || stylizedCounters == 2\">\n                                    </div>\n                                    <div class=\"card-icon\" mbr-if=\"stylizedCounters == 3\">\n                                        <span mbr-icon=\"\" class=\"amp-iconfont fa-code fa\"></span>\n                                    </div>\n                                    <div class=\"card-row\" mbr-if=\"showCardListTitle || showCardListText\">\n                                        <h4 class=\"card-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-7\" mbr-if=\"showCardListTitle\" data-app-selector=\".mbr-list .card-title\" mbr-class=\"{'mbr-pb-2':showCardListText}\">\n                                            No Coding\n                                        </h4>\n                                        <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardListText\" data-app-selector=\".mbr-list .mbr-text\">\n                                           Lorem ipsum dolor sit amet, consectetur adipisicing elit.\n                                        </p>\n                                     </div>\n                                </div> \n                            <div class=\"mbr-flex\">\n                                    <div class=\"card-marker\" mbr-if=\"stylizedCounters == 1 || stylizedCounters == 2\">\n                                    </div>\n                                    <div class=\"card-icon\" mbr-if=\"stylizedCounters == 3\">\n                                        <span mbr-icon=\"\" class=\"amp-iconfont fa-code fa\"></span>\n                                    </div>\n                                    <div class=\"card-row\" mbr-if=\"showCardListTitle || showCardListText\">\n                                        <h4 class=\"card-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-7\" mbr-if=\"showCardListTitle\" data-app-selector=\".mbr-list .card-title\" mbr-class=\"{'mbr-pb-2':showCardListText}\">\n                                            No Coding\n                                        </h4>\n                                        <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardListText\" data-app-selector=\".mbr-list .mbr-text\">\n                                           Lorem ipsum dolor sit amet, consectetur adipisicing elit.\n                                        </p>\n                                     </div>\n                                </div></div>\n                            <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showCardText\" data-app-selector=\".card-box .mbr-text\" mbr-class=\"{'mbr-pt-3':showCardTitle || showCardSubtitle || showCardList}\">\n                               Lorem ipsum dolor sit amet, consectetur adipisicing elit.\n                            </p>\n                            <div class=\"infNumerically mbr-row mbr-px-2\" mbr-if=\"showInfNumerically\" mbr-list=\"\" mbr-list-grow=\"infCount\" mbr-class=\"{'mbr-pt-4':!infBorderTop &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText), 'align-center': columnWidth == 5}\">\n                                <div class=\"mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-lg-6 mbr-col-xl-4':infCount == 3 &amp;&amp; fullWidth &amp;&amp; columnWidth != 5, 'mbr-col-md-6 mbr-col-lg-6': infCount > 1  &amp;&amp; columnWidth != 5, 'mbr-col-xl-6': infCount > 1 &amp;&amp; infCount != 3 &amp;&amp; fullWidth &amp;&amp; columnWidth != 5, 'mbr-col-md-12 mbr-col-lg-12': infCount == 1 || columnWidth == 5}\">\n                                    <div class=\"numeral mbr-flex\" mbr-class=\"{'mbr-jc-c': cardAlign == 2 || columnWidth == 5, 'mbr-jc-e': cardAlign == 3 &amp;&amp; columnWidth != 5}\">\n                                        <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".infNumerically .numeral\">\n                                            100\n                                        </h4>\n                                        <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" data-app-selector=\".infNumerically .numeral\">\n                                            %\n                                        </h4>\n                                    </div>\n                                    <p class=\"mbr-text mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-7\" data-app-selector=\".infNumerically .mbr-text\">\n                                       Lorem ipsum\n                                    </p>\n                                </div>\n                            \n                        <div class=\"mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-lg-6 mbr-col-xl-4':infCount == 3 &amp;&amp; fullWidth &amp;&amp; columnWidth != 5, 'mbr-col-md-6 mbr-col-lg-6': infCount > 1  &amp;&amp; columnWidth != 5, 'mbr-col-xl-6': infCount > 1 &amp;&amp; infCount != 3 &amp;&amp; fullWidth &amp;&amp; columnWidth != 5, 'mbr-col-md-12 mbr-col-lg-12': infCount == 1 || columnWidth == 5}\">\n                                    <div class=\"numeral mbr-flex\" mbr-class=\"{'mbr-jc-c': cardAlign == 2 || columnWidth == 5, 'mbr-jc-e': cardAlign == 3 &amp;&amp; columnWidth != 5}\">\n                                        <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".infNumerically .numeral\">\n                                            100\n                                        </h4>\n                                        <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" data-app-selector=\".infNumerically .numeral\">\n                                            %\n                                        </h4>\n                                    </div>\n                                    <p class=\"mbr-text mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-7\" data-app-selector=\".infNumerically .mbr-text\">\n                                       Lorem ipsum\n                                    </p>\n                                </div><div class=\"mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-lg-6 mbr-col-xl-4':infCount == 3 &amp;&amp; fullWidth &amp;&amp; columnWidth != 5, 'mbr-col-md-6 mbr-col-lg-6': infCount > 1  &amp;&amp; columnWidth != 5, 'mbr-col-xl-6': infCount > 1 &amp;&amp; infCount != 3 &amp;&amp; fullWidth &amp;&amp; columnWidth != 5, 'mbr-col-md-12 mbr-col-lg-12': infCount == 1 || columnWidth == 5}\">\n                                    <div class=\"numeral mbr-flex\" mbr-class=\"{'mbr-jc-c': cardAlign == 2 || columnWidth == 5, 'mbr-jc-e': cardAlign == 3 &amp;&amp; columnWidth != 5}\">\n                                        <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".infNumerically .numeral\">\n                                            100\n                                        </h4>\n                                        <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" data-app-selector=\".infNumerically .numeral\">\n                                            %\n                                        </h4>\n                                    </div>\n                                    <p class=\"mbr-text mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-7\" data-app-selector=\".infNumerically .mbr-text\">\n                                       Lorem ipsum\n         