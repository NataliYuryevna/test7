      "components": [
        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value"
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".twoBg": {
              "position": "absolute",
              "background-color": "@twoColor",
              "top": "0",
              "left": "0",
              "width": "100%",
              "height": "100%",
              "padding": "0",
              "z-index": "0"
            },
            ".card-wrapper, .card-wrapper .card-box, .card-wrapper .card-icon": {
              "position": "relative",
              "z-index": "2"
            },
            ".card-wrapper when (@showImg)": {
              "height": "~\"@{imgHeight}rem\"",
              ".card-img:before": {
                "content": "\"\"",
                "height": "100%",
                "width": "100%",
                "opacity": "0.5",
                "z-index": "1",
                "pointer-events": "none"
              },
              ".card-img": {
                "&, &:before": {
                  "position": "absolute",
                  "top": "0",
                  "left": "0"
                },
                "&, img, amp-img": {
                  "width": "100%",
                  "height": "~\"@{imgHeight}rem\""
                }
              },
              "@media (max-width: 991px)": {
                "&, .card-img, .card-img img, .card-img amp-img": {
                  "height": "100%"
                }
              }
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor",
              ".mbr-overlay": {
                "background": "transparent",
                "opacity": "1"
              }
            },
            ".card-wrapper when not (@transparentBg)": {
              ".mbr-overlay, .card-img:before": {
                "background-color": "@cardBg",
                "opacity": "@cardOpacity"
              }
            },
            ".card-wrapper when (@showIcons), (@showNumber)": {
              ".card-icon": {
                "display": "inline-block",
                ".amp-iconfont": {
                  "font-size": "~\"@{iconsSize}rem\"",
                  "color": "@iconsColor !important",
                  "@media (max-width: 767px)": {
                    "font-size": "50px"
                  }
                }
              },
              ".card-box when not (@fullWidth)": {
                "padding-top": "5px"
              }
            },
            ".card-wrapper when (@fullWidth)": {
              "padding-left": "60px",
              "padding-right": "60px"
            },
            ".card-wrapper when not (@transparentBg), (@showBorders), (@showCardsEffect), (@fullWidth)": {
              "@media (max-width: 991px)": {
                "padding": "15px"
              }
            },
            ".card-title when (@showLine)": {
              "display": "inline-block",
              "&:after": {
                "content": "\"\"",
                "display": "block",
                "background-color": "@primaryColor",
                "height": "3px",
                "width": "40px",
                "& when (@cardsAlign = \"2\")": {
                  "margin": "0 auto"
                },
                "& when (@cardsAlign = \"3\")": {
                  "margin-left": "auto"
                },
                "margin-top": "15px",
                "transition": "all .2s ease"
              }
            },
            ".card-wrapper:hover .card-title:after & when (@showLineEffect) and (@showLine)": {
              "width": "100%"
            },
            "& when (@showCardsEffect)": {
              ".card-wrapper when not (@showBorders) and (@transparentBg)": {
                "&, .card-icon .amp-iconfont, .card-box .card-title": {
                  "transition": "all 0.2s linear"
                },
                "&:hover": {
                  "& when (@effect = \"1\")": {
                    "box-shadow": "0px 0px 40px 1px rgba(0, 0, 0, 0.1)"
                  },
                  "& when (@effect = \"2\")": {
                    "background-color": "#f0f1f5"
                  },
                  ".card-icon .amp-iconfont, .card-box .card-title, .card-icon .card-number": {
                    "color": "@primaryColor !important"
                  }
                }
              },
              ".card-wrapper when (@showBorders), not (@transparentBg)": {
                ".mbr-overlay, .card-icon .amp-iconfont, .card-box, .card-box .btn": {
                  "transition": "all 0.2s linear"
                },
                "&:hover": {
                  ".mbr-overlay, .card-img:before": {
                    "background": "@primaryColor"
                  },
                  ".card-icon .amp-iconfont, .card-icon .card-number, .card-box, .btn[class*=\"-outline\"], .card-box .btn[class*=\"-outline\"]:hover": {
                    "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                  },
                  ".card-box .card-title:after, .card-icon .card-marker": {
                    "background-color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                  }
                }
              }
            },
            "@media (max-width: 991px)": {
