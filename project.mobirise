              },
              ".nav-item:hover": {
                ".nav-link, .dropdown-item:hover": {
                  "color": "@primaryColor !important"
                },
                ".nav-link:after": {
                  "border-color": "@primaryColor !important"
                }
              },
              ".dropdown-item, .nav-item:not(:last-child) .nav-link": {
                "border-bottom": "1px solid #bec1c7"
