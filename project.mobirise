          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".text-content": {
              "overflow": "hidden"
            },
            ".card-box, .card-container, .carousel-img, .dots-wrapper, .container-img.img-with-overlay:hover amp-img:before": {
              "width": "100%"
            },
            ".card-container, .card-wrap, .image-wrap, .slider-box, .container-img.img-with-overlay:hover amp-img": {
              "position": "relative"
            },
            ".mbr-row when (@showPadding)": {
              "@media (min-width: 991px)": {
                ".card-wrap": {
                  "padding-right": "70px"
                },
                ".mbr-row-reverse .card-wrap": {
                  "padding-right": "15px",
                  "padding-left": "70px"
                }
              },
              "@media (min-width: 1200px)": {
                ".card-container .card-wrap when (@fullWidth)": {
                  "padding-left": "100px",
                  "padding-right": "100px"
                }
              }
            },
            ".card-container when not (@transparentBg)": {
              ".mbr-overlay": {
                "background": "@cardBg",
                "opacity": "@cardOpacity"
              },
              ".image-wrap": {
                "margin": "0"
              },
              ".image-wrap, .carousel-img.mbr-col-md-12": {
                "padding": "0"
              }
            },
            ".infNumerically:before when (@infBorderTop) and (@showInfNumerically)": {
              "content": "\"\"",
              "width": "100%",
              "height": "1px",
              "background-color": "@borderColor",
              "margin": "50px 0"
            },
            ".card-marker:before, .mbr-text:before": {
              "display": "inline-block",
              "vertical-align": "middle"
            },
            ".mbr-list when (@stylizedCounters = \"1\")": {
              "counter-reset": "decimal",
              ".card-marker:before": {
                "counter-increment": "decimal",
                "content": "\"0\" counter(decimal)",
                "color": "@primaryColor",
                "font-size": "~\"@{widthImg}rem\"",
                "& when (@countersBorder)": {
                  "border-bottom": "2px solid @primaryColor"
                }
              }
            },
            ".card-marker:before when (@stylizedCounters = \"2\")": {
              "content": "\"\"",
              "border": "3px solid @primaryColor",
              "width": "~\"calc(@{widthImg}rem - 6px)\"",
              "height": "~\"calc(@{widthImg}rem - 6px)\"",
              "box-sizing": "initial"
            },
            ".mbr-list .mbr-flex": {
              "flex-direction": "row",
              ".card-marker, .card-icon": {
                "margin-right": "15px"
              },
              "&:not(:last-child)": {
                "padding-bottom": "10px"
              }
            },
            ".card-icon .amp-iconfont": {
              "color": "@primaryColor",
              "font-size": "~\"@{widthImg}rem\""
            },
            "amp-carousel": {
              "height": "~\"@{slideHeight}px\" !important"
            },
            ".carousel-img": {
              "height": "~\"calc(@{slideHeight}px - 85px)\"",
              "margin": "0 0 85px 0",
              ".container-img": {
                "& when not (@showImgTitle) and not (@showImgText)": {
                  "height": "~\"calc(@{slideHeight}px - 85px)\""
                },
                "& when (@showImgTitle), (@showImgText)": {
                  "height": "~\"calc(@{slideHeight}px - 185px)\""
                },
                "amp-img,img": {
                  "object-fit": "cover"
                }
              },
              ".text-wrap": {
                "height": "100px",
                "padding-top": "35px",
                ".mbr-text:before when (@lineText)": {
                  "content": "''",
                  "width": "25px",
                  "height": "3px",
                  "margin-right": "10px",
                  "background-color": "@primaryColor"
                }
              }
            },
            "@media (min-width: 768px)": {
              ".infNumerically > div:not(:first-child) when (@infBorderRight) and not (@columnWidth = 5)": {
                "border-left": "1px solid @borderColor"
              },
              ".infNumerically > div:not(:last-child) when (@columnWidth = 5)": {
                "margin-bottom": "20px"
              },
              ".infNumerically when (@infCount > 2) and not (@columnWidth = 5)": {
                "& > div:nth-child(2n+1)": {
                  "border-left": "none"
                },
                "& > div:not(:last-child)": {
                  "margin-bottom": "20px"
                },
                "& > div:nth-last-child(2) when (@infCount = 4)": {
                  "margin-bottom": "0px"
                }
              }
            },
            ".amp-carousel-button": {
              "background-color": "@buttonsColor",
              "top": "~\"calc(100% - 27px)\"",
              "opacity": "0.8",
              "&:after": {
                "border-color": "contrast(@buttonsColor, darken(@buttonsColor, 50%), #ffffff, 60%) !important"
              },
              "&:hover": {
                "opacity": "1"
              },
              "&.amp-carousel-button-prev": {
                "left": "0"
              },
              "&.amp-carousel-button-next": {
                "left": "55px"
              },
              "@media (min-width: 992px)": {
                "& when not (@countImg = 1), (@transparentBg)": {
                  "&.amp-carousel-button-prev": {
                    "left": "15px"
                  },
                  "&.amp-carousel-button-next": {
                    "left": "(15px + 55px)"
                  }
                }
              },
              "@media (min-width: 1200px)": {
                "& when (@countImg = 2)": {
                  "&.amp-carousel-button-prev": {
                    "left": "30px"
                  },
                  "&.amp-carousel-button-next": {
                    "left": "(30px + 55px)"
                  }
                }
              }
            },
            ".card-box .infNumerically > div when (@infCount = 3) and (@fullWidth) and not (@columnWidth = 5)": {
              "@media (min-width: 1200px)": {
                "margin-bottom": "0px",
                "&:not(:first-child) when (@infBorderRight)": {
                  "border-left": "1px solid @borderColor"
                }
              }
            },
            "@media (max-width: 991px)": {
              ".card-wrap when (@transparentBg)": {
                "padding-bottom": "50px"
              }
            },
            "@media (max-width: 767px)": {
              ".infNumerically": {
                "text-align": "center",
                ".numeral": {
                  "justify-content": "center"
                },
                "& > div:not(:last-child)": {
                  "margin-bottom": "20px"
                }
              }
            },
            ".container-img.img-with-overlay:hover amp-img": {
              "&:before": {
                "content": "\"\"",
                "position": "absolute",
                "height": "100%",
                "background-color": "@imgOverlayColor",
                "opacity": "@imgOverlayOpacity",
                "z-index": "1",
                "pointer-events": "none"
              },
              "& > a:before when (@showImgButton)": {
                "content": "\"\"",
                "position": "absolute",
                "top": "50%",
                "left": "50%",
                "font-size": "40px",
                "transform": "translate(-50%, -50%)",
                "color": "contrast(@imgOverlayColor, darken(@imgOverlayColor, 50%), #ffffff, 60%)",
                "z-index": "2"
              }
            }
          },
          "_name": "slider2",
          "_customHTML": "<section class=\"slider2 slider\" group=\"Sliders\" plugins=\"amp-main, amp-carousel\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Width Images\" name=\"columnWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showInfNumerically\">    \n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showInfNumerically\">\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked=\"\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>    \n    <header>Images</header>\n        <input type=\"range\" inline=\"\" title=\"Slider Height\" name=\"slideHeight\" min=\"300\" max=\"700\" step=\"10\" value=\"350\">\n        <input type=\"range\" inline=\"\" title=\"Count Images Slider\" name=\"countImg\" min=\"1\" max=\"4\" step=\"1\" value=\"3\">\n        <select title=\"Sliders Count\" name=\"slidersAmount\">\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\" selected=\"\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\">8</option>\n        </select>\n        <input type=\"color\" title=\"Buttons Color\" name=\"buttonsColor\" value=\"#273272\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showImgTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showImgText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Line Before Text\" name=\"lineText\" condition=\"showImgText\" checked=\"\">\n        <select title=\"Align\" name=\"textImgAlign\" condition=\"showImgTitle || showImgText\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Image Overlay\" name=\"showImgOverlay\" checked=\"\">\n        <input type=\"color\" title=\"Image Overlay Color\" name=\"imgOverlayColor\" value=\"#000000\" condition=\"showImgOverlay\">\n        <input type=\"range\" inline=\"\" title=\"Image Overlay Opacity\" name=\"imgOverlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"showImgOverlay\">\n        <input type=\"checkbox\" title=\"Show Image Button\" name=\"showImgButton\" condition=\"showImgOverlay\" checked=\"\">\n    <header>Text</header>    \n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showCardSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showCardText\" checked=\"\">\n        <input type=\"checkbox\" title=\"List\" name=\"showCardList\" checked=\"\">\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"listCount\" min=\"1\" max=\"9\" step=\"1\" value=\"1\" condition=\"showCardList\"> \n            <select title=\"Stylized Counters\" name=\"stylizedCounters\" condition=\"showCardList\">\n                <option value=\"1\">Counters</option>\n                <option value=\"2\" selected=\"\">Markers</option>\n                <option value=\"3\">Icons</option>\n            </select>\n            <input type=\"checkbox\" title=\"Border Bottom Counters\" name=\"countersBorder\" checked=\"\" condition=\"showCardList &amp;&amp; stylizedCounters == 1\">\n            <input type=\"range\" inline=\"\" title=\"Width Marker\" name=\"widthImg\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"0.8\" condition=\"showCardList\">\n            <input type=\"checkbox\" title=\"Title List\" name=\"showCardListTitle\" condition=\"showCardList\">\n            <input type=\"checkbox\" title=\"Text List\" name=\"showCardListText\" checked=\"\" condition=\"showCardList\">\n        <input type=\"checkbox\" title=\"Information Numerically\" name=\"showInfNumerically\" checked=\"\">\n            <input type=\"checkbox\" title=\"Border Top\" name=\"infBorderTop\" condition=\"showInfNumerically\" checked=\"\">\n            <input type=\"checkbox\" title=\"Border Right\" name=\"infBorderRight\" condition=\"showInfNumerically &amp;&amp; infCount != 1 &amp;&amp; columnWidth != 5\" checked=\"\">\n            <input type=\"color\" title=\"Color Border\" name=\"borderColor\" value=\"#bec1c7\" condition=\"showInfNumerically &amp;&amp; (infBorderTop || (infBorderRight &amp;&amp; infCount != 1))\">\n            <input type=\"range\" inline=\"\" title=\"Count Information\" name=\"infCount\" min=\"1\" max=\"4\" step=\"1\" value=\"4\" condition=\"showInfNumerically\">\n        <select title=\"Align\" name=\"cardAlign\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showInfNumerically\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Padding\" name=\"showPadding\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showInfNumerically\" checked=\"\">\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardTitle || showCardSubtitle || showCardText || showCardList || showInfNumerically\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#bec1c7\" condition=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardText || showCardList || showInfNumerically)\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardText || showCardList || showInfNumerically)\">\n    <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\" checked=\"\"> \n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=\"width2Bg\" min=\"1\" max=\"5\" step=\"1\" value=\"3\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">    \n        <fieldset type=\"background\" name=\"bg\">\n            <input type=\"image\" title=\"Background Image\" value=\"../_images/background2.jpg\">\n            <input type=\"color\" title=\"Background Color\" value=\"#f4f5f7\" selected=\"\">\n        </fieldset>\n        <input type=\"color\" title=\"2 Color\" name=\"twoColor\" value=\"#ffffff\" condition=\"twoBgColor &amp;&amp; bg.type == 'color'\">  \n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked=\"\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.6\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"(overlay &amp;&amp; bg.type !== 'color') || (twoBgColor &amp;&amp; bg.type == 'color')\" mbr-class=\"{'mbr-col-md-4': width2Bg == 1 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-5': width2Bg == 2 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-6': width2Bg == 3 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-7': width2Bg == 4 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color',\n                     'mbr-col-md-8': width2Bg == 5 &amp;&amp; twoBgColor &amp;&amp; bg.type == 'color'}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"title-wrap mbr-pb-4\" mbr-if=\"showTitle || showSubtitle\" mbr-class=\"{'align-left': titleAlign==1,\n                         'align-center': titleAlign==2,\n                         'align-right': titleAlign==3, 'mbr-px-2': fullWidth}\">\n            <h1 class=\"mbr-section-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">\n                Slider</h1>\n            <h2 class=\"mbr-section-subtitle mbr-fonts-style text-danger\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">Lorem ipsum dolor sit amet, consectetur adipisicing elit.</h2>\n        </div> \n        <div class=\"mbr-row\" mbr-class=\"{'mbr-px-2': !transparentBg}\">\n            <div class=\"card-container mbr-flex flex-wrap mbr-jc-c\" mbr-class=\"{'mbr-row-reverse':reverseContent &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showInfNumerically)}\">\n                <div class=\"mbr-overlay\" mbr-if=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText || showInfNumerically)\"></div>\n                <div class=\"card-wrap mbr-flex mbr-col-sm-12 mbr-col-md-12\" mbr-if=\"showCardTitle || showCardSubtitle || showCardText || showInfNumerically || showCardList\" mbr-class=\"{'mbr-col-lg-8':columnWidth==1,\n                                     'mbr-col-lg-7':columnWidth==2,\n                                     'mbr-col-lg-6':columnWidth==3,\n                                     'mbr-col-lg-5':columnWidth==4,\n                                     'mbr-col-lg-4':columnWidth==5}\">\n                        <div class=\"card-box mbr-m-auto\" mbr-class=\"{'align-center': cardAlign==2,\n                                     'align-right': cardAlign==3, 'mbr-py-4': !transparentBg, 'mbr-px-2': transparentBg &amp;&amp; fullWidth}\">\n                            <h2 class=\"card-title mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" mbr-if=\"showCardTitle\" data-app-selector=\".card-title\">About Our</h2>\n                            <h3 class=\"card-subtitle mbr-fonts-style mbr-bold text-primary\" mbr-theme-style=\"display-5\" mbr-if=\"showCardSubtitle\" data-app-selector=\".card-subtitle\">Company</h3>\n                            <div class=\"mbr-list\" mbr-list=\"\" mbr-list-grow=\"listCount\" mbr-if=\"showCardList\" mbr-class=\"{'mbr-pt-4': showCardTitle || showCardSubtitle}\">\n                                <div class=\"mbr-flex\">\n                                    <div class=\"card-marker\" mbr-if=\"stylizedCounters == 1 || stylizedCounters == 2\">\n                                    </div>\n                                    <div class=\"card-icon\" mbr-if=\"stylizedCounters == 3\">\n                                        <span mbr-icon=\"\" class=\"amp-iconfont fa-code fa\"></span>\n                                    </div>\n                                    <div class=\"card-row\" mbr-if=\"showCardListTitle || showCardListText\">\n                                        <h4 class=\"card-title mbr-fonts-style mbr-bold text-danger\" mbr-theme-style=\"display-7\" mbr-if=\"showCardListTitle\" data-app-selector=\".mbr-list .card-title\" mbr-class=\"{'mbr-pb-2':showCardListText}\">\n                                            No Coding\n                                        </h4>\n                                        <p class=\"mbr-text mbr-fonts-style text-danger\" mbr-theme-style=\"display-7\" mbr-if=\"showCardListText\" data-app-selector=\".mbr-list .mbr-text\">\n                                           Lorem ipsum dolor sit amet, consectetur adipisicing elit.\n                                        </p>\n                                     </div>\n                                </div> \n                            </div>\n                            <p class=\"mbr-text mbr-fonts-style text-danger\" mbr-theme-style=\"display-7\" mbr-if=\"showCardText\" data-app-selector=\".card-box .mbr-text\" mbr-class=\"{'mbr-pt-3':showCardTitle || showCardSubtitle || showCardList}\">\n                               Lorem ipsum dolor sit amet, consectetur adipisicing elit.\n                            </p>\n                            <div class=\"infNumerically mbr-row mbr-px-2\" mbr-if=\"showInfNumerically\" mbr-list=\"\" mbr-list-grow=\"infCount\" mbr-class=\"{'mbr-pt-4':!infBorderTop &amp;&amp; (showCardTitle || showCardSubtitle || showCardList || showCardText), 'align-center': columnWidth == 5}\">\n                                <div class=\"mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-6 mbr-col-lg-6 mbr-col-xl-4':infCount == 3 &amp;&amp; fullWidth &amp;&amp; columnWidth != 5, 'mbr-col-md-6 mbr-col-lg-6': infCount > 1  &amp;&amp; columnWidth != 5, 'mbr-col-xl-6': infCount > 1 &amp;&amp; infCount != 3 &amp;&amp; fullWidth &amp;&amp; columnWidth != 5, 'mbr-col-md-12 mbr-col-lg-12': infCount == 1 || columnWidth == 5}\">\n                                    <div class=\"numeral mbr-flex\" mbr-class=\"{'mbr-jc-c': cardAlign == 2 || columnWidth == 5, 'mbr-jc-e': cardAlign == 3 &amp;&amp; columnWidth != 5}\">\n                                        <h4 class=\"mbr-fonts-style mbr-bold\" mbr-