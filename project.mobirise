            ".second-element-box, .container-elements": {
              "width": "100%"
            },
            ".container-elements, .second-element, .image-wrap": {
              "position": "relative"
            },
            ".second-element when (@showPadding)": {
              "@media (min-width: 992px)": {
                "& when not (@reverseContent)": {
                  "padding-left": "70px"
                },
                "& when (@reverseContent)": {
                  "padding-right": "70px"
                }
              },
              "@media (min-width: 1200px)": {
                "& when (@fullWidth)": {
                  "padding-left": "100px",
                  "padding-right": "100px"
                }
              }
            },
            ".container-elements when not (@transparentBg)": {
              ".mbr-overlay": {
                "background": "@cardBg",
                "opacity": "@cardOpacity"
              },
              ".image-wrap": {
                "padding": "0"
              }
            },
            ".milestone:before when (@showBorderTop) and (@showMilestone)": {
              "content": "\"\"",
              "width": "100%",
              "height": "1px",
              "background-color": "@borderColor",
              "margin": "50px 0"
            },
            "amp-img,img": {
              "object-fit": "cover",
              "@media (min-width: 768px)": {
                "height": "~\"@{heightImg}rem\"",
                "& when (@fullScreen) and not (@showTitle) and not (@showSubtitle)": {
                  "height": "100vh"
                }
              }
            },
            ".marker-wrap:before": {
              "display": "inline-block"
            },
            ".mbr-list when (@stylizedCounters = \"1\")": {
              "counter-reset": "decimal",
              ".marker-wrap:before": {
                "counter-increment": "decimal",
                "content": "\"0\" counter(decimal)",
                "color": "@primaryColor",
                "font-size": "~\"@{widthImg}rem\"",
                "line-height": "1.7",
                "& when (@countersBorder)": {
                  "border-bottom": "2px solid @primaryColor",
                  "line-height": "1.4"
                }
              }
            },
            ".marker-wrap:before when (@stylizedCounters = \"2\")": {
              "content": "\"\"",
              "border": "3px solid @primaryColor",
              "width": "~\"calc(@{widthImg}rem - 6px)\"",
              "height": "~\"calc(@{widthImg}rem - 6px)\"",
              "box-sizing": "initial",
              "margin-top": "9px"
            },
            ".mbr-list .items-list": {
              "flex-direction": "row",
              ".marker-wrap, .icon-wrap": {
                "margin-right": "15px"
              },
              "&:not(:last-child)": {
                "padding-bottom": "10px"
              }
            },
            ".icon-wrap .amp-iconfont": {
              "color": "@primaryColor",
              "font-size": "~\"@{widthImg}rem\""
            },
            "@media (min-width: 768px)": {
              ".milestone > div:not(:first-child) when (@showBorderRight) and not (@columnWidth = 5)": {
                "border-left": "1px solid @borderColor"
              },
              ".milestone > div:not(:last-child) when (@columnWidth = 5)": {
                "margin-bottom": "20px"
              },
              ".milestone when (@milCount > 2) and not (@columnWidth = 5)": {
                "& > div:nth-child(2n+1)": {
                  "border-left": "none"
                },
                "& > div:not(:last-child)": {
                  "margin-bottom": "20px"
                },
                "& > div:nth-last-child(2) when (@milCount = 4)": {
                  "margin-bottom": "0px"
                }
              }
            },
            ".second-element-box .milestone > div when (@milCount = 3) and (@fullWidth) and not (@columnWidth = 5)": {
              "@media (min-width: 1200px)": {
                "margin-bottom": "0px",
                "&:not(:first-child) when (@showBorderRight)": {
                  "border-left": "1px solid @borderColor"
                }
              }
            },
            "@media (max-width: 767px)": {
              ".second-element when (@transparentBg)": {
                "padding-top": "50px"
              },
              ".milestone": {
                "text-align": "center",
                ".numeral": {
                  "justify-content": "center"
                },
                "& > div:not(:last-child)": {
                  "margin-bottom": "20px"
                }
              }
            }
