        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@fullHeight) and not (@showTitle) and not (@showSubtitle)": {
              "padding": "0",
              ".mbr-row .card": {
                ".card-img, .card-img img, .card-img amp-img, .card-box": {
                  "min-height": "450px",
                  "@media (max-width: 991px)": {
                    "min-height": "350px"
                  }
                },
                ".card-box when not (@showImg)": {
                  "@media (min-width: 992px)": {
                    "min-height": "900px"
                  }
                },
                ".card-box when not (@showImg) and (@transparentBg) and not (@showBorders)": {
                  "min-height": "270px"
                }
              }
            },
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".card-benefits-box": {
              "align-items": "center",
              ".card-number when (@showLineNumber)": {
                "position": "relative",
                "padding-left": "70px",
                "& when not (@transparentBg), (@showBorders)": {
                  "& when not (@fullWidth)": {
                    "padding-left": "30px"
                  }
                },
                "&:before": {
                  "content": "''",
                  "width": "50px",
                  "& when not (@transparentBg), (@showBorders)": {
                    "& when not (@fullWidth)": {
                      "width": "10px"
                    }
                  },
                  "height": "1px",
                  "position": "absolute",
                  "left": "10px",
                  "top": "50%",
                  "-webkit-transform": "translateY(-50%)",
                  "transform": "translateY(-50%)",
                  "background-color": "@lineColor"
                }
              }
            },
            ".card-box": {
              "width": "100%",
              "& when (@showBorders), not (@transparentBg)": {
                "padding": "60px",
                "@media (max-width: 991px)": {
                  "padding": "25px"
                }
              }
            },
            ".card-wrapper, .card-wrapper .card-box": {
              "position": "relative"
            },
            ".card-wrapper .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-wrapper when (@showCardsEffect)": {
              "&:hover .card-benefits-box, .card-subtitle, .mbr-text, .card-btn": {
                "max-height": "0",
                "opacity": "0"
              },
              ".card-benefits-box, &:hover .card-subtitle, &:hover .mbr-text, &:hover .card-btn": {
                "max-height": "1000px",
                "opacity": "1"
              },
              ".card-benefits-box": {
                "transition": "all 0.2s linear 0.2s"
              },
              ".card-subtitle, .mbr-text, .card-btn": {
                "transition": "all 0.2s linear"
              },
              "&:hover when not (@transparentBg)": {
                ".mbr-overlay": {
                  "background": "@primaryColor",
                  "opacity": "@cardOpacity"
                },
                ".card-benefits-box h4, .card-text-box, .card-box .btn[class*=\"-outline\"], .card-box .btn[class*=\"-outline\"]:hover": {
                  "color": "contrast(@primaryColor, darken(@primaryColor, 50%), #ffffff, 60%) !important"
                }
              }
            },
            ".mbr-row .card when not (@showPadding)": {
              "padding": "0",
              "& when (@showBorders)": {
                ".card-wrapper": {
                  "border": "none"
                },
                "@media (min-width: 768px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "1px solid @bordersColor"
                  },
                  "@media (max-width: 991px)": {
                    "&.md-pb .card-wrapper": {
                      "border-bottom": "1px solid @bordersColor"
                    },
                    "&.md-pb:nth-child(2) .card-wrapper": {
                      "border-right": "none"
                    }
                  }
                },
                "@media (max-width: 767px)": {
                  "&:not(.last-child) .card-wrapper": {
                    "border-right": "none",
                    "border-bottom": "1px solid @bordersColor"
                  }
                }
              }
            }
          },
          "_name": "features8",
          "_customHTML": "<section class=\"features8\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n            <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"!(fullHeight &amp;&amp; !showTitle &amp;&amp; !showSubtitle)\">\n            <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"!(fullHeight &amp;&amp; !showTitle &amp;&amp; !showSubtitle)\">\n            <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n            <input type=\"checkbox\" title=\"Cards Full Height\" name=\"fullHeight\" condition=\"!showTitle &amp;&amp; !showSubtitle\" checked=\"\">\n        <header>Title</header>\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\">\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n            <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n                <option value=\"1\" selected=\"\">Left</option>\n                <option value=\"2\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n        \n        <header>Cards</header>\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"4\" condition=\"showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\">\n            <input type=\"checkbox\" title=\"Show Title Number\" name=\"showCardsTitleNumber\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Number\" name=\"showCardsNumber\" checked=\"\" condition=\"showCardsTitleNumber\">\n            <input type=\"checkbox\" title=\"Show Line Before Number\" name=\"showLineNumber\" condition=\"showCardsTitleNumber &amp;&amp; showCardsNumber\" checked=\"\">\n            <input type=\"color\" title=\"Line Color\" name=\"lineColor\" value=\"#9399B9\" condition=\"showCardsTitleNumber &amp;&amp; showCardsNumber &amp;&amp; showLineNumber\">\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showCardsSubtitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Text\" name=\"showCardsText\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Buttons\" name=\"showCardsButtons\" checked=\"\">\n            <select title=\"Align\" name=\"cardsAlign\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons\">\n                <option value=\"1\" selected=\"\">Left</option>\n                <option value=\"2\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n            <input type=\"checkbox\" title=\"Show Image\" name=\"showImg\">\n            <input type=\"checkbox\" title=\"Show Effect\" name=\"showCardsEffect\" condition=\"(showCardsTitleNumber || showCardsTitle) &amp;&amp; (showCardsSubtitle || showCardsText || showCardsButtons)\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Padding\" name=\"showPadding\" condition=\"showCardsTitleNumber || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\">\n            <input type=\"checkbox\" title=\"Borders\" name=\"showBorders\" condition=\"showCardsTitleNumber || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\" checked=\"\">\n            <input type=\"color\" title=\"Color\" name=\"bordersColor\" value=\"#efefef\" condition=\"showBorders &amp;&amp; (showCardsTitleNumber || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg)\">\n            <input type=\"checkbox\" t