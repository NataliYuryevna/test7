          "_customHTML": "<section class=\"features9\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n            <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"!(fullHeight &amp;&amp; !showTitle &amp;&amp; !showSubtitle)\">\n            <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\" condition=\"!(fullHeight &amp;&amp; !showTitle &amp;&amp; !showSubtitle)\">\n            <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked=\"\">\n            <input type=\"checkbox\" title=\"Cards Full Height\" name=\"fullHeight\" condition=\"!showTitle &amp;&amp; !showSubtitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Chess Style\" name=\"chessStyle\" condition=\"showImg\" checked=\"\">\n        <header>Title</header>\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\">\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n            <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n                <option value=\"1\" selected=\"\">Left</option>\n                <option value=\"2\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n        \n        <header>Cards</header>\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"4\" condition=\"showCardsInfo || showCardsCategory || showCardsDate || showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\">\n            <input type=\"checkbox\" title=\"Show Title Number\" name=\"showCardsTitleNumber\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Number\" name=\"showCardsNumber\" checked=\"\" con