                      "padding-right": "90px",
                      "& when (@fullWidth)": {
                        "@media (min-width: 1200px)": {
                          "padding-left": "14%"
                        }
                      }
                    },
                    "&.mbr-col-lg-5,&.mbr-col-lg-4": {
                      "padding-right": "100px",
                      "& when (@fullWidth)": {
                        "@media (min-width: 1200px)": {
                          "padding-left": "13%"
                        }
                      }
                    }
                  },
                  "& when not (@reverseContent)": {
                    "&.mbr-col-lg-7,&.mbr-col-lg-8": {
                      "padding-left": "70px",
                      "& when (@fullWidth)": {
                        "@media (min-width: 1200px)": {
