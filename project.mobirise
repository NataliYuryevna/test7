          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            "amp-carousel .slide": {
              "align-self": "flex-start"
            },
            ".slider-box, amp-carousel": {
              "height": "~\"@{slideHeight}px\"",
              "position": "relative"
            },
            ".slide, .slider-box, amp-carousel, .dots-wrapper": {
              "width": "100%"
            },
            ".dots span, .amp-carousel-button": {
              "background-color": "@buttonsColor"
            },
            ".amp-carousel-button, .dots span": {
              "-webkit-transition": "all 0.3s",
              "transition": "all 0.3s"
            },
            "amp-img,img": {
              "object-fit": "cover"
            },
            "& when not (@typeSlide = 'carousel')": {
              ".slide": {
                "height": "~\"calc(@{slideHeight}px - 35px)\"",
                "margin-bottom": "35px"
              },
              ".dots-wrapper": {
                "position": "absolute",
                "bottom": "0"
              },
              ".dots span": {
                "display": "inline-block",
                "vertical-align": "middle",
                "border-radius": "10px",
                "width": "9px",
                "height": "9px",
                "margin": "0 3px",
                "opacity": "0.4",
                "outline": "none"
              },
              ".dots span.current": {
                "opacity": "1",
                "width": "18px"
              },
              ".amp-carousel-button": {
                "display": "none"
              }
            },
            "& when not (@showBullets), (@typeSlide = 'carousel')": {
              ".slide when (@typeSlide = 'carousel'), (@slideWidth = 0), not (@showCenter)": {
                "height": "~\"calc(@{slideHeight}px - 85px)\"",
                "margin": "0 0 85px 0"
              },
              ".slide when (@typeSlide = 'slides') and not (@slideWidth = 0) and (@showCenter)": {
                "height": "~\"@{slideHeight}px\""
              },
              ".amp-carousel-button": {
                "display": "block",
                "opacity": "0.8",
                "&:before, &:after": {
                  "color": "contrast(@buttonsColor, darken(@buttonsColor, 50%), #ffffff, 60%) !important"
                },
                "&:hover": {
                  "opacity": "1"
                }
              },
              ".amp-carousel-button when (@typeSlide = 'carousel'), (@slideWidth = 0), not (@showCenter)": {
                "top": "~\"calc(100% - 27px)\"",
                "&.amp-carousel-button-prev": {
                  "left": "15px"
                },
                "&.amp-carousel-button-next": {
                  "left": "70px"
                },
                "@media (min-width: 1200px)": {
                  "& when (@cardsAmount = 2) and (@typeSlide = 'carousel')": {
                    "&.amp-carousel-button-prev": {
                      "left": "30px"
                    },
                    "&.amp-carousel-button-next": {
                      "left": "(30px + 55px)"
                    }
                  }
                }
              },
              ".dots-wrapper": {
                "display": "none"
              },
              "@media (max-width: 767px)": {
                "& when not (@showBullets) and not (@typeSlide = 'carousel')": {
                  ".dots-wrapper": {
                    "display": "flex"
                  },
                  ".amp-carousel-button": {
                    "display": "none"
                  },
                  ".slide": {
                    "height": "~\"calc(@{slideHeight}px - 35px)\"",
                    "margin-bottom": "35px"
                  }
                }
              }
            }
