            },
            ".mbr-pb-3": {
              "padding-bottom": "10px"
            },
            ".mbr-pb-5": {
              "padding-bottom": "30px"
            },
            ".mbr-pt-5": {
              "padding-top": "30px"
            },
            ".mbr-pt-4": {
              "padding-top": "20px"
            },
            ".mbr-px-4": {
              "padding-left": "20px",
              "padding-right": "20px"
            },
            ".mbr-py-6": {
              "padding-top": "30px",
              "padding-bottom": "30px"
            },
            "& when (@stylizedCounters = \"1\")": {
              ".mbr-list": {
                "counter-reset": "decimal"
              },
              ".card-marker:before": {
                "counter-increment": "decimal",
                "content": "\"0\" counter(decimal)",
                "color": "@primaryColor",
                "font-size": "~\"@{widthImg}rem\"",
                "line-height": "1.6",
                "& when (@countersBorder)": {
                  "border-bottom": "3px solid @primaryColor"
                }
              }
            },
            "& when (@stylizedCounters = \"2\")": {
              ".mbr-list": {
                "list-style-type": "none"
              },
              ".card-marker:before": {
                "content": "\"\"",
                "border": "3px solid @primaryColor",
                "width": "~\"calc(@{widthImg}rem - 6px)\"",
                "height": "~\"calc(@{widthImg}rem - 6px)\"",
                "box-sizing": "initial",
                "display": "inline-block",
                "vertical-align": "middle"
              }
            },
            ".mbr-list .mbr-flex": {
              "flex-direction": "row",
              "align-items": "flex-start",
              ".card-marker, .card-icon": {
                "margin-right": "15px"
              },
              "&:not(:last-child)": {
                "padding-bottom": "10px"
              }
            },
            ".card-icon .amp-iconfont": {
              "color": "@primaryColor",
              "font-size": "~\"@{widthImg}rem\"",
              "line-height": "1.6"
            },
            ".image-wrap amp-img img": {
              "object-fit": "cover"
            },
            "& when (@showPadding)": {
