        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".mbr-row when (@cardsAmount = 2)": {
              "@media (min-width: 1200px)": {
                "margin": "0 -30px",
                "&.mbr-px-2": {
                  "padding-left": "30px",
                  "padding-right": "30px"
                }
              }
            },
            ".milestone > div > div": {
              "padding-left": "0",
              "padding-right": "0"
            },
            ".card-title:after when (@showLine)": {
              "content": "\"\"",
              "display": "block",
              "width": "25px",
              "height": "3px",
              "margin": "inherit",
              "margin-top": "18px",
              "background-color": "@primaryColor"
            },
            ".milestone, .milestone > div, .milestone > div > div": {
              "width": "100%"
            },
            ".milestone > div > div when (@showBorderBottom)": {
              "border-bottom": "1px solid @milBorderColor"
            },
            ".milestone .mbr-text": {
              "padding-top": "5px"
            },
            ".title when (@showTitle), (@showSubtitle)": {
              "position": "relative",
              ".milestone > div when (@milCount > 2)": {
                "&:nth-child(1), &:nth-child(2)": {
                  "& when (@titleAlign = \"2\")": {
                    "@media (max-width: 991px)": {
                      "padding-bottom": "20px"
                    }
                  },
                  "& when not (@titleAlign = \"2\")": {
                    "padding-bottom": "20px"
                  }
                }
              },
              ".milestone when (@titleAlign = \"2\")": {
                "@media (min-width: 768px)": {
                  "position": "absolute",
                  "top": "0",
                  "& > div:first-child when (@milCount < 4)": {
                    "margin-right": "auto"
                  },
                  "@media (min-width: 991px)": {
                    "& > div:nth-child(2) when (@milCount = 4)": {
                      "margin-right": "auto"
                    }
                  }
                }
              }
            },
            "@media (max-width: 767px)": {
              ".title-text, .milestone > div:not(:last-child)": {
                "padding-bottom": "20px"
              },
              ".milestone": {
                "text-align": "center",
                ".numeral": {
                  "justify-content": "center"
                }
              }
            },
            ".card-wrapper, .card-wrapper .card-box": {
              "position": "relative"
            },
