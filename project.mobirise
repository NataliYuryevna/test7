        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".mbr-row when (@cardsAmount = 2)": {
              "@media (min-width: 1200px)": {
                "margin": "0 -30px",
                "&.mbr-px-2": {
                  "padding": "0 30px"
                }
              }
            },
            ".infNumerically > div > div": {
              "padding-left": "0",
              "padding-right": "0"
            },
            ".card-box .line when (@showLine)": {
              "&:after": {
                "content": "\"\"",
                "display": "inline-block",
                "width": "25px",
                "height": "3px",
                "margin-top": "18px",
                "line-height": "0",
                "background-color": "@primaryColor"
              }
            },
            ".infNumerically, .infNumerically > div, .infNumerically > div > div": {
              "width": "100%"
            },
            ".infNumerically > div > div when (@infBorderBottom)": {
              "border-bottom": "1px solid @infBorderColor"
            },
            ".infNumerically .mbr-text": {
              "padding-top": "5px"
            },
            ".title when (@showTitle), (@showSubtitle)": {
              "position": "relative",
              ".infNumerically > div when (@infCount > 2)": {
                "&:nth-child(1), &:nth-child(2)": {
                  "& when (@titleAlign = \"2\")": {
                    "@media (max-width: 991px)": {
                      "padding-bottom": "20px"
                    }
                  },
                  "& when not (@titleAlign = \"2\")": {
                    "padding-bottom": "20px"
                  }
                }
              },
              ".infNumerically when (@titleAlign = \"2\")": {
                "@media (min-width: 768px)": {
                  "position": "absolute",
                  "top": "0",
                  "& > div:first-child when (@infCount < 4)": {
                    "margin-right": "auto"
                  },
                  "@media (min-width: 991px)": {
                    "& > div:nth-child(2) when (@infCount = 4)": {
                      "margin-right": "auto"
                    }
                  }
                }
              }
            },
            "@media (max-width: 767px)": {
              ".title-text, .infNumerically > div:not(:last-child)": {
                "padding-bottom": "20px"
              },
              ".infNumerically": {
                "text-align": "center",
                ".numeral": {
                  "justify-content": "center"
                }
              }
            },
            ".card-wrapper, .card-wrapper .card-box": {
              "position": "relative"
            },
            ".card-wrapper .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".card-wrapper when (@showBorders)": {
              "border": "1px solid @bordersColor"
            },
            ".card-img.img-with-overlay:hover amp-img": {
              "position": "relative",
              "&:before": {
                "content": "\"\"",
                "position": "absolute",
                "height": "100%",
                "width": "100%",
                "background-color": "@imgOverlayColor",
                "opacity": "@imgOverlayOpacity",
                "z-index": "1",
                "pointer-events": "none"
              },
              "& > a:before when (@showImgButton)": {
                "content": "\"\"",
                "position": "absolute",
                "top": "50%",
                "left": "50%",
                "font-size": "40px",
                "transform": "translate(-50%, -50%)",
                "color": "contrast(@imgOverlayColor, darken(@imgOverlayColor, 50%), #ffffff, 60%)",
                "z-index": "2"
              }
            }
          },
          "_name": "features3",
          "_customHTML": "<section class=\"features3\" group=\"Features\" plugins=\"amp-main\">\n\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Size</header>\n            <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n            <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n            <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <header>Title</header>\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showTitle\" checked=\"\">\n            <input type=\"checkbox\" title=\"Show Subtitle\" name=\"showSubtitle\">\n            <input type=\"checkbox\" title=\"Information Numerically\" name=\"showInfNumerically\" checked=\"\">\n                <input type=\"checkbox\" title=\"Border Bottom\" name=\"infBorderBottom\" condition=\"showInfNumerically\" checked=\"\">\n                <input type=\"color\" title=\"Border Color\" name=\"infBorderColor\" value=\"#d4d6e3\" condition=\"showInfNumerically &amp;&amp; infBorderBottom\" checked=\"\">\n                <input type=\"range\" inline=\"\" title=\"Count Information\" name=\"infCount\" min=\"1\" max=\"4\" step=\"1\" value=\"2\" condition=\"showInfNumerically\">\n            <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle || showSubtitle\">\n                <option value=\"1\" selected=\"\">Left</option>\n                <option value=\"2\">Center</option>\n                <option value=\"3\">Right</option>\n            </select>\n        \n        <header>Cards</header>\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"cardsAmount\" min=\"1\" max=\"4\" step=\"1\" value=\"3\" condition=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\">\n            <input type=\"checkbox\" title=\"Show Title\" name=\"showCardsTit