step=\"0.1\" value=\"0.5\" condition=\"overlay &amp;&amp; bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay &amp;&amp; bg.type !== 'color'\"></div>\n    <div class=\"twoBg\" mbr-if=\"twoBgColor &amp;&amp; bg.type == 'color'\" mbr-class=\"{'mbr-col-md-4 mbr-col-lg-4': width2Bg == 1,\n                     'mbr-col-md-5 mbr-col-lg-5': width2Bg == 2,\n                     'mbr-col-md-6 mbr-col-lg-6': width2Bg == 3,\n                     'mbr-col-md-7 mbr-col-lg-7': width2Bg == 4,\n                     'mbr-col-md-8 mbr-col-lg-8': width2Bg == 5}\"></div>\n    <div mbr-class=\"{'container':!fullWidth,\n                     'container-fluid':fullWidth}\">\n        <div class=\"title mbr-pb-6\" mbr-if=\"showTitle || showSubtitle || showInfNumerically\" mbr-class=\"{'mbr-flex flex-wrap': showInfNumerically &amp;&amp; (showTitle || showSubtitle) &amp;&amp; titleAlign != 2, 'mbr-row-reverse':showInfNumerically &amp;&amp; (showTitle || showSubtitle) &amp;&amp; titleAlign == 3, 'mbr-col-sm-12': fullWidth}\">\n            <div class=\"title-text\" mbr-class=\"{'align-left': titleAlign == 1, 'align-center': titleAlign == 2, 'align-right': titleAlign == 3, 'mbr-col-md-4 mbr-px-0': showInfNumerically, 'mbr-m-auto':showInfNumerically &amp;&amp; titleAlign == 2}\" mbr-if=\"showTitle || showSubtitle\">        \n                <h2 class=\"mbr-section-title mbr-semibold mbr-fonts-style\" mbr-class=\"{'mbr-pb-2':showSubtitle}\" mbr-theme-style=\"display-5\" mbr-if=\"showTitle\" data-app-selector=\".mbr-section-title\">\n                    Features with Images\n                </h2>\n                <h3 class=\"mbr-section-subtitle mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-5\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">\n                    Features with Images Subtitle\n                </h3>\n            </div>\n            <div class=\"infNumerically mbr-flex flex-wrap mbr-px-0\" mbr-if=\"showInfNumerically\" mbr-list=\"\" mbr-list-grow=\"infCount\" mbr-class=\"{'mbr-col-md-8': titleAlign != 2 &amp;&amp; (showTitle || showSubtitle),'mbr-jc-c': infCount != 3 &amp;&amp; (titleAlign != 2 &amp;&amp; (showTitle || showSubtitle) || (!showTitle &amp;&amp; !showSubtitle)), 'mbr-row': !showTitle &amp;&amp; !showSubtitle}\">\n                <div mbr-class=\"{'mbr-col-md-6 mbr-col-lg-6 mbr-col-xl-4':infCount == 3 &amp;&amp; fullWidth &amp;&amp; titleAlign != 2 &amp;&amp; (showTitle || showSubtitle), 'mbr-col-md-6 mbr-col-lg-6': infCount > 1 &amp;&amp; titleAlign != 2 &amp;&amp; (showTitle || showSubtitle), 'mbr-col-xl-6': infCount > 1 &amp;&amp; infCount != 3 &amp;&amp; fullWidth &amp;&amp; titleAlign != 2 &amp;&amp; (showTitle || showSubtitle), 'mbr-col-md-6 mbr-col-lg-2': titleAlign == 2 &amp;&amp; (showTitle || showSubtitle), 'mbr-col-md-3 mbr-col-lg-3': !showTitle &amp;&amp; !showSubtitle}\">\n                    <div class=\"mbr-px-0 mbr-m-auto\" mbr-class=\"{'mbr-col-md-4 mbr-col-lg-12': titleAlign == 2 &amp;&amp; (showTitle || showSubtitle), 'mbr-pb-6': infBorderBottom, 'mbr-col-md-8': (titleAlign != 2 &amp;&amp; (showTitle || showSubtitle)) || (!showTitle &amp;&amp; !showSubtitle)}\">\n                        <div class=\"numeral mbr-flex\">\n                            <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".infNumerically .numeral\">\n                                100\n                            </h4>\n                            <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" data-app-selector=\".infNumerically .numeral\">\n                                %\n                            </h4>\n                        </div>\n                        <p class=\"mbr-text mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-7\" data-app-selector=\".infNumerically .mbr-text\">\n                           Lorem ipsum\n                        </p>\n                    </div>\n                </div>\n            <div mbr-class=\"{'mbr-col-md-6 mbr-col-lg-6 mbr-col-xl-4':infCount == 3 &amp;&amp; fullWidth &amp;&amp; titleAlign != 2 &amp;&amp; (showTitle || showSubtitle), 'mbr-col-md-6 mbr-col-lg-6': infCount > 1 &amp;&amp; titleAlign != 2 &amp;&amp; (showTitle || showSubtitle), 'mbr-col-xl-6': infCount > 1 &amp;&amp; infCount != 3 &amp;&amp; fullWidth &amp;&amp; titleAlign != 2 &amp;&amp; (showTitle || showSubtitle), 'mbr-col-md-6 mbr-col-lg-2': titleAlign == 2 &amp;&amp; (showTitle || showSubtitle), 'mbr-col-md-3 mbr-col-lg-3': !showTitle &amp;&amp; !showSubtitle}\">\n                    <div class=\"mbr-px-0 mbr-m-auto\" mbr-class=\"{'mbr-col-md-4 mbr-col-lg-12': titleAlign == 2 &amp;&amp; (showTitle || showSubtitle), 'mbr-pb-6': infBorderBottom, 'mbr-col-md-8': (titleAlign != 2 &amp;&amp; (showTitle || showSubtitle)) || (!showTitle &amp;&amp; !showSubtitle)}\">\n                        <div class=\"numeral mbr-flex\">\n                            <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-5\" data-app-selector=\".infNumerically .numeral\">\n                                100\n                            </h4>\n                            <h4 class=\"mbr-fonts-style mbr-bold\" mbr-theme-style=\"display-6\" data-app-selector=\".infNumerically .numeral\">\n                                %\n                            </h4>\n                        </div>\n                        <p class=\"mbr-text mbr-fonts-style mbr-semibold\" mbr-theme-style=\"display-7\" data-app-selector=\".infNumerically .mbr-text\">\n                           Lorem ipsum\n                        </p>\n                    </div>\n                </div></div>\n        </div>\n        <div class=\"mbr-row mbr-jc-c\" mbr-if=\"showCardsTitle || showCardsSubtitle || showCardsText || showCardsButtons || showImg\">\n            <div class=\"card mbr-col-sm-12\" mbr-class=\"{'mbr-col-md-12': cardsAmount == 1,\n                                          'mbr-col-md-6