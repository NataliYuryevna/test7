            ".card-title-box": {
              "justify-content": "space-between",
              ".category-wrap > *": {
                "display": "inline-block",
                "padding-right": "14px"
              },
              ".category-wrap when (@showLinePost)": {
                ".card-category, .card-date": {
                  "&:before": {
                    "content": "''",
                    "display": "inline-block",
                    "height": "0",
                    "width": "0",
                    "background-color": "#9EA1A9",
                    "transition": "all .2s ease"
                  },
                  "&:hover:before when (@showEffectLinePost)": {
                    "transform": "rotate(90deg)",
                    "background-color": "@primaryColor"
                  }
                },
                ".card-category:before when (@showCardsInfo)": {
                  "height": "12px",
                  "width": "1px",
                  "margin-right": "14px"
                },
                ".card-date:before when (@showCardsCategory), (@showCardsInfo)": {
                  "height": "12px",
                  "width": "1px",
                  "margin-right": "14px"
                }
              }
            },
            "@media (min-width: 768px)": {
              "& when (@showLineText)": {
                ".card-text-box, .card-title-box": {
                  "padding-left": "50px"
                },
                ".card-text-box": {
                  "position": "relative",
                  "&:before": {
                    "content": "\"\"",
                    "position": "absolute",
                    "top": "10px",
                    "left": "0",
                    "width": "30px",
                    "height": "3px",
                    "background-color": "@primaryColor"
                  }
                }
              }
            },
            ".infNumerically, .infNumerically > div, .infNumerically > div > div, .card-box": {
              "width": "100%"
            },
            ".infNumerically > div > div when (@infBorderBottom)": {
              "border-bottom": "1px solid @infBorderColor"
            },
            ".infNumerically .mbr-text": {
              "padding-top": "5px"
            },
            ".title when (@showTitle), (@showSubtitle)": {
              "position": "relative",
              ".infNumerically > div when (@infCount > 2)": {
                "&:nth-child(1), &:nth-child(2)": {
                  "& when (@titleAlign = \"2\")": {
                    "@media (max-width: 991px)": {
                      "padding-bottom": "20px"
                    }
                  },
                  "& when not (@titleAlign = \"2\")": {
                    "padding-bottom": "20px"
                  }
                }
              },
              ".infNumerically when (@titleAlign = \"2\")": {
                "@media (min-width: 768px)": {
                  "position": "absolute",
                  "top": "0",
                  "& > div:first-child when (@infCount < 4)": {
                    "margin-right": "auto"
                  },
                  "@media (min-width: 991px)": {
                    "& > div:nth-child(2) when (@infCount = 4)": {
                      "margin-right": "auto"
                    }
                  }
                }
              }
            },
            "@media (max-width: 767px)": {
              ".title-text, .infNumerically > div:not(:last-child)": {
                "padding-bottom": "20px"
              },
              ".infNumerically": {
                "text-align": "center",
                ".numeral": {
                  "justify-content": "center"
                }
              }
            },
            ".card-img, .twoBg": {
