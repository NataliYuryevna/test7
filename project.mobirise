        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".container-img, .text-wrap": {
              "height": "100%"
            },
            ".card-container, .slider-box, .slide, .dots-wrapper": {
              "width": "100%"
            },
            ".card-container, .card-wrap, .image-wrap, .slider-box, amp-carousel": {
              "position": "relative"
            },
            ".text-wrap": {
              "& when not (@testimonialsBg)": {
                "background-color": "@textBg"
              },
              ".mbr-section-title:before when (@lineTitle)": {
                "content": "''",
                "width": "25px",
                "height": "3px",
                "margin-right": "10px",
                "background-color": "@primaryColor"
              }
            },
            ".slider-box, amp-carousel": {
              "height": "(@slideHeight * 1px)"
            },
            ".mbr-row when (@showPadding)": {
              "@media (min-width: 991px)": {
                ".card-wrap": {
                  "padding-right": "70px"
                },
                ".mbr-row-reverse .card-wrap": {
                  "padding-left": "70px",
                  "padding-right": "15px"
                }
              },
              "@media (min-width: 1200px)": {
                ".card-container .card-wrap when (@fullWidth)": {
                  "padding-left": "100px",
                  "padding-right": "100px"
                }
              }
            },
            ".container-img": {
              "padding": "0"
            },
            ".card-container .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".infNumerically:before when (@infBorderTop) and (@showInfNumerically)": {
              "content": "\"\"",
              "width": "100%",
              "height": "1px",
              "background-color": "@borderColor",
              "margin": "50px 0"
            },
            ".card-marker:before, .dots span, .mbr-section-title:before": {
              "display": "inline-block",
              "vertical-align": "middle"
            },
            ".mbr-list when (@stylizedCounters = \"1\")": {
              "counter-reset": "decimal",
              ".card-marker:before": {
                "counter-increment": "decimal",
                "content": "\"0\" counter(decimal)",
                "color": "@primaryColor",
                "font-size": "~\"@{widthImg}rem\"",
                "& when (@countersBorder)": {
                  "border-bottom": "2px solid @primaryColor"
                }
              }
            },
            ".card-marker:before when (@stylizedCounters = \"2\")": {
              "content": "\"\"",
              "border": "3px solid @primaryColor",
              "width": "~\"calc(@{widthImg}rem - 6px)\"",
              "height": "~\"calc(@{widthImg}rem - 6px)\"",
              "box-sizing": "initial"
            },
            ".mbr-list .mbr-flex": {
              "flex-direction": "row",
              ".card-marker, .card-icon": {
                "margin-right": "15px"
              },
              "&:not(:last-child)": {
                "padding-bottom": "10px"
              }
            },
            ".card-icon .amp-iconfont": {
              "color": "@primaryColor",
              "font-size": "~\"@{widthImg}rem\""
            },
            "amp-carousel .slide, .slide .card-row": {
              "align-self": "flex-start"
            },
            ".slide": {
              "height": "~\"calc(@{slideHeight}px - 35px)\"",
              "margin-bottom": "35px",
              "amp-img,img": {
                "object-fit": "cover"
              },
              ".container-img img": {
                "-webkit-transition": "transform 5s linear",
                "transition": "transform 5s linear"
              },
              "&:hover .container-img img": {
                "-webkit-transform": "scale(1.15)",
                "transform": "scale(1.15)"
              }
            },
            ".dots-wrapper": {
              "position": "absolute",
              "bottom": "0"
            },
            ".dots span, .amp-carousel-button": {
              "background-color": "@buttonsColor"
            },
            ".dots span": {
              "border-radius": "10px",
              "width": "9px",
              "height": "9px",
              "margin": "0 3px",
              "opacity": "0.4",
              "outline": "none"
            },
            ".dots span.current": {
              "opacity": "1",
              "width": "18px"
            },
            ".amp-carousel-button": {
              "display": "none"
            },
            "@media (min-width: 768px)": {
              ".infNumerically > div:not(:first-child) when (@infBorderRight) and not (@columnWidth = 5)": {
                "border-left": "1px solid @borderColor"
              },
              ".infNumerically > div:not(:last-child) when (@columnWidth = 5)": {
                "margin-bottom": "20px"
              },
              ".infNumerically when (@infCount > 2) and not (@columnWidth = 5)": {
                "& > div:nth-child(2n+1)": {
                  "border-left": "none"
                },
                "& > div:not(:last-child)": {
                  "margin-bottom": "20px"
                },
                "& > div:nth-last-child(2) when (@infCount = 4)": {
                  "margin-bottom": "0px"
                }
              },
              "& when not (@showBullets)": {
                ".slide": {
                  "height": "~\"calc(@{slideHeight}px - 85px)\"",
                  "margin-bottom": "85px"
                },
                ".amp-carousel-button": {
                  "display": "block",
                  "top": "~\"calc(100% - 27px)\"",
                  "opacity": "0.8",
                  "&:after": {
                    "border-color": "contrast(@buttonsColor, darken(@buttonsColor, 50%), #ffffff, 60%) !important"
                  },
                  "&:hover": {
                    "opacity": "1"
                  }
                },
                ".amp-carousel-button-prev": {
                  "left": "0"
                },
                ".amp-carousel-button-next": {
                  "left": "55px"
                },
                ".dots-wrapper": {
                  "display": "none"
                }
              }
            },
            ".card-box .infNumerically > div when (@infCount = 3) and (@fullWidth) and not (@columnWidth = 5)": {
              "@media (min-width: 1200px)": {
                "margin-bottom": "0px",
                "&:not(:first-child) when (@infBorderRight)": {
                  "border-left": "1px solid @borderColor"
                }
              }
            },
            "@media (max-width: 767px)": {
              ".container-img when (@showImgText), (@showImgTitle)": {
                "height": "50%"
              },
              ".text-wrap when (@showImg)": {
                "height": "50%"
              },
              ".card-wrap when (@transparentBg)": {
                "& when (@showImgText), (@showImgTitle), (@showImg)": {
                  "padding-bottom": "50px"
                }
              },
              ".text-wrap when (@showImg) and (@testimonialsBg)": {
                "padding-top": "25px"
              },
              ".infNumerically": {
                "text-align": "center",
                ".numeral": {
                  "justify-content": "center"
                },
                "& > div:not(:last-child)": {
                  "margin-bottom": "20px"
                }
              }
            }
          },
          "_name": "slider4",
          "_customHTML": "<section class=\"slider4 slider slider-with-bullets\" group=\"Sliders\" plugins=\"amp-main, amp-carousel, amp-bind\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"4\">\n        <input type=\"range\" inline=\"\" title=\"Width Images\" name=\"columnWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"(showCardTitle || showCardSubtitle || showCardList || showCardText || showInfNumerically) &amp;&amp; (showImgText || showImgTitle || showImg)\">    \n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"(showCardTitle || showCardSubtitle || showCardList || showCardText || showInfNumerically) &amp;&amp; (showImgText || showImgTitle || showImg)\">\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked=\"\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>    \n    <header>Testimonials</header>\n        <input type=\"range\" inline=\"\" title=\"Width Testimonials\" name=\"testimonialsWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"(showImgText || showImgTitle) &amp;&amp; showImg\">\n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"testimonialsReverseContent\" condition=\"(showImgText || showImgTitle) &amp;&amp; showImg\">\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\" checked=\"\" condition=\"slidersAmount > 1 &amp;&amp; (showImgText || showImgTitle || showImg)\">\n            <input type=\"range\" inline=\"\" title=\"Delay\" name=\"delay\" min=\"1000\" max=\"5000\" step=\"1000\" value=\"3000\" condition=\"autoplay &amp;&amp; slidersAmount > 1 &amp;&amp; (showImgText || showImgTitle || showImg)\">\n        <input type=\"range\" inline=\"\" title=\"Slider Height\" name=\"slideHeight\" min=\"300\" max=\"700\" step=\"10\" value=\"480\" condition=\"showImgText || showImgTitle || showImg\">\n        <select title=\"Sliders Count\" name=\"slidersAmount\" condition=\"showImgText || showImgTitle || showImg\">\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\" selected=\"\">8</option>\n        </select>\n        <input type=\"checkbox\" title=\"Show Buttons/Bullets\" name=\"showBullets\" condition=\"showImgText || showImgTitle || showImg\">\n            <input type=\"color\" title=\"Color\" name=\"buttonsColor\" value=\"#273272\" condition=\"showImgText || showImgTitle || showImg\">\n        <input type=\"checkbox\" title=\"Image\" name=\"showImg\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showImgText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showImgTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Line Before Title\" name=\"lineTitle\" condition=\"showImgTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"testimonialsBg\" condition=\"showImgText || showImgTitle\">\n        <input type=\"color\" title=\"Color\" name=\"textBg\" value=\"#ffffff\" condition=\"!testimonialsBg &amp;&amp; (showImgText || showImgTitle)\">\n        <select title=\"Align\" 