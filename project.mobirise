              "@media (min-width: 1200px)": {
                "margin-bottom": "0px",
                "&:not(:first-child) when (@infBorderRight)": {
                  "border-left": "1px solid @borderColor"
                }
              }
            },
            "@media (max-width: 991px)": {
              ".card-wrap when (@transparentBg)": {
                "padding-bottom": "50px"
              }
            },
            "@media (max-width: 767px)": {
              ".infNumerically": {
                "text-align": "center",
                ".numeral": {
                  "justify-content": "center"
                },
                "& > div:not(:last-child)": {
                  "margin-bottom": "20px"
                }
              }
            },
            ".container-img.img-with-overlay:hover amp-img": {
              "&:before": {
                "content": "\"\"",
                "position": "absolute",
