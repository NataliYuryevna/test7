                  "&.mbr-row-reverse .card-wrap": {
                    "padding-left": "70px",
                    "padding-right": "15px"
                  }
                }
              },
              "@media (min-width: 1200px)": {
                ".container-fluid .mbr-row .card-wrap": {
                  "padding-left": "100px",
                  "padding-right": "100px"
                }
              }
            },
            ".card-container when not (@transparentBg)": {
              ".mbr-overlay": {
                "background": "@cardBg",
                "opacity": "@cardOpacity"
              },
              ".image-wrap": {
                "margin": "0"
              },
              ".image-wrap, .carousel-img.mbr-col-md-12": {
                "padding": "0"
              }
            },
            ".infNumerically:before when (@infBorderTop) and (@showInfNumerically)": {
              "content": "\"\"",
              "width": "100%",
              "height": "1px",
              "background-color": "@borderColor",
              "margin": "50px 0"
            },
            ".card-marker:before": {
              "display": "inline-block",
              "vertical-align": "middle"
            },
            ".mbr-list when (@stylizedCounters = \"1\")": {
              "counter-reset": "decimal",
              ".card-marker:before": {
                "counter-increment": "decimal",
                "content": "\"0\" counter(decimal)",
                "color": "@primaryColor",
                "font-size": "~\"@{widthImg}rem\"",
                "& when (@countersBorder)": {
                  "border-bottom": "2px solid @primaryColor"
