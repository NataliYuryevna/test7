        {
          "_styles": {
            "padding-top": "(@paddingTop * 15px)",
            "padding-bottom": "(@paddingBottom * 15px)",
            "& when (@bg-type = 'color')": {
              "background-color": "@bg-value",
              ".mbr-overlay when (@twoBgColor)": {
                "padding": "0",
                "background": "@twoColor",
                "opacity": "1"
              }
            },
            "& when (@bg-type = 'image')": {
              "background-image": "url(@bg-value)",
              ".mbr-overlay": {
                "background": "@overlayColor",
                "opacity": "@overlayOpacity"
              }
            },
            ".container-elements, .slider-box, .slide, .dots-wrapper, .second-element-box, .text-content": {
              "width": "100%"
            },
            ".container-img, .text-wrap, .text-box": {
              "height": "100%"
            },
            ".container-elements, .second-element, .image-wrap, .slider-box, amp-carousel": {
              "position": "relative"
            },
            ".text-wrap": {
              "& when not (@testimonialsBg)": {
                "background-color": "@textBg",
                "padding": "0"
              },
              ".mbr-content-subtitle:before when (@lineTitle)": {
                "content": "''",
                "width": "25px",
                "height": "3px",
                "margin-right": "10px",
                "vertical-align": "middle",
                "background-color": "@primaryColor"
              }
            },
            ".slider-box, amp-carousel": {
              "height": "(@slideHeight * 1px)"
            },
            ".text-box when (@testimonialsPadding) and (@showImg)": {
              "@media (min-width: 992px)": {
                "& when not (@testimonialsReverseContent)": {
                  "padding-left": "50px"
                },
                "& when (@testimonialsReverseContent)": {
                  "padding-right": "50px"
                }
              }
            },
            ".second-element when (@showPadding)": {
              "& when (@showImgText), (@showImgTitle), (@showImg)": {
                "@media (min-width: 992px)": {
                  "& when not (@reverseContent)": {
                    "padding-right": "70px"
                  },
                  "& when (@reverseContent)": {
                    "padding-left": "70px"
                  }
                },
                "@media (min-width: 1200px)": {
                  "& when (@fullWidth)": {
                    "padding-left": "100px",
                    "padding-right": "100px"
                  }
                }
              }
            },
            ".container-img": {
              "padding": "0"
            },
            ".container-elements .mbr-overlay when not (@transparentBg)": {
              "background": "@cardBg",
              "opacity": "@cardOpacity"
            },
            ".milestone:before when (@showBorderTop) and (@showMilestone)": {
              "content": "\"\"",
              "width": "100%",
              "height": "1px",
              "background-color": "@borderColor",
              "margin": "50px 0"
            },
            ".marker-wrap:before, .dots span, .mbr-content-subtitle:before": {
              "display": "inline-block"
            },
            ".mbr-list when (@stylizedCounters = \"1\")": {
              "counter-reset": "decimal",
              ".marker-wrap:before": {
                "counter-increment": "decimal",
                "content": "\"0\" counter(decimal)",
                "color": "@primaryColor",
                "line-height": "1.7",
                "& when (@countersBorder)": {
                  "border-bottom": "2px solid @primaryColor",
                  "line-height": "1.4"
                }
              }
            },
            ".marker-wrap:before when (@stylizedCounters = \"2\")": {
              "content": "\"\"",
              "border": "3px solid @primaryColor",
              "width": "~\"calc(@{widthImg}rem - 6px)\"",
              "height": "~\"calc(@{widthImg}rem - 6px)\"",
              "box-sizing": "initial",
              "margin-top": "9px"
            },
            ".mbr-list .items-list": {
              "flex-direction": "row",
              ".marker-wrap, .icon-wrap": {
                "margin-right": "15px"
              },
              "&:not(:last-child)": {
                "padding-bottom": "10px"
              }
            },
            ".icon-wrap .amp-iconfont": {
              "color": "@primaryColor",
              "font-size": "~\"@{widthImg}rem\""
            },
            "amp-carousel .slide": {
              "align-self": "flex-start"
            },
            ".slide": {
              "height": "~\"calc(@{slideHeight}px - 35px)\"",
              "margin-bottom": "35px",
              "amp-img,img": {
                "object-fit": "cover"
              },
              ".container-img img": {
                "-webkit-transition": "transform 5s linear",
                "transition": "transform 5s linear"
              },
              "&:hover .container-img img": {
                "-webkit-transform": "scale(1.15)",
                "transform": "scale(1.15)"
              }
            },
            ".dots-wrapper": {
              "position": "absolute",
              "bottom": "0"
            },
            ".dots span, .amp-carousel-button": {
              "background-color": "@buttonsColor"
            },
            ".dots span": {
              "border-radius": "10px",
              "width": "9px",
              "height": "9px",
              "margin": "0 3px",
              "opacity": "0.4",
              "outline": "none"
            },
            ".dots span.current": {
              "opacity": "1",
              "width": "18px"
            },
            ".amp-carousel-button": {
              "display": "none"
            },
            "@media (min-width: 768px)": {
              ".milestone > div:not(:first-child) when (@showBorderRight) and not (@columnWidth = 5)": {
                "border-left": "1px solid @borderColor"
              },
              ".milestone > div:not(:last-child) when (@columnWidth = 5)": {
                "margin-bottom": "20px"
              },
              ".milestone when (@milCount > 2) and not (@columnWidth = 5)": {
                "& > div:nth-child(2n+1)": {
                  "border-left": "none"
                },
                "& > div:not(:last-child)": {
                  "margin-bottom": "20px"
                },
                "& > div:nth-last-child(2) when (@milCount = 4)": {
                  "margin-bottom": "0px"
                }
              },
              ".second-element-box .milestone > div when (@milCount = 3) and (@fullWidth) and not (@columnWidth = 5)": {
                "@media (min-width: 1200px)": {
                  "margin-bottom": "0px",
                  "&:not(:first-child) when (@showBorderRight)": {
                    "border-left": "1px solid @borderColor"
                  }
                }
              },
              "& when not (@showBullets)": {
                ".slide": {
                  "height": "~\"calc(@{slideHeight}px - 85px)\"",
                  "margin-bottom": "85px"
                },
                ".amp-carousel-button": {
                  "display": "block",
                  "top": "~\"calc(100% - 27px)\"",
                  "opacity": "0.8",
                  "&:after": {
                    "border-color": "contrast(@buttonsColor, darken(@buttonsColor, 50%), #ffffff, 60%) !important"
                  },
                  "&:hover": {
                    "opacity": "1"
                  }
                },
                ".amp-carousel-button-prev": {
                  "left": "0px"
                },
                ".amp-carousel-button-next": {
                  "left": "55px"
                },
                ".dots-wrapper": {
                  "display": "none"
                }
              }
            },
            "@media (max-width: 767px)": {
              ".container-img when (@showImgText), (@showImgTitle)": {
                "height": "50%"
              },
              ".text-wrap when (@showImg)": {
                "height": "50%"
              },
              ".second-element when (@transparentBg)": {
                "& when (@showImgText), (@showImgTitle), (@showImg)": {
                  "padding-bottom": "50px"
                }
              },
              ".text-wrap when (@showImg) and (@testimonialsBg)": {
                "padding-top": "25px"
              },
              ".milestone": {
                "text-align": "center",
                ".numeral": {
                  "justify-content": "center"
                },
                "& > div:not(:last-child)": {
                  "margin-bottom": "20px"
                }
              }
            }
          },
          "_name": "slider4",
          "_customHTML": "<section class=\"slider4 slider slider-with-bullets\" group=\"Sliders\" plugins=\"amp-main, amp-carousel, amp-bind\">\n    <mbr-parameters>\n    <!-- Block parameters controls (Blue \"Gear\" panel) -->\n    <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\">\n        <input type=\"range\" inline=\"\" title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"10\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline=\"\" title=\"Width Images\" name=\"columnWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"(showCardTitle || showCardSubtitle || showCardList || showCardText || showCardButtons) &amp;&amp; (showImgText || showImgTitle || showImg)\">    \n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"reverseContent\" condition=\"(showCardTitle || showCardSubtitle || showCardList || showCardText || showCardButtons) &amp;&amp; (showImgText || showImgTitle || showImg)\">\n    <header>Title</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\">\n        <select title=\"Align\" name=\"titleAlign\" condition=\"showTitle\">\n            <option value=\"1\">Left</option>\n            <option value=\"2\" selected=\"\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>    \n    <header>Testimonials</header>\n        <input type=\"range\" inline=\"\" title=\"Width Testimonials\" name=\"testimonialsWidth\" min=\"1\" max=\"5\" step=\"1\" value=\"4\" condition=\"(showImgText || showImgTitle) &amp;&amp; showImg\">\n        <input type=\"checkbox\" title=\"Reverse Content\" name=\"testimonialsReverseContent\" condition=\"(showImgText || showImgTitle) &amp;&amp; showImg\">\n        <input type=\"checkbox\" title=\"Autoplay\" name=\"autoplay\" checked=\"\" condition=\"slidersAmount > 1 &amp;&amp; (showImgText || showImgTitle || showImg)\">\n            <input type=\"range\" inline=\"\" title=\"Delay\" name=\"delay\" min=\"1000\" max=\"5000\" step=\"1000\" value=\"3000\" condition=\"autoplay &amp;&amp; slidersAmount > 1 &amp;&amp; (showImgText || showImgTitle || showImg)\">\n        <input type=\"range\" inline=\"\" title=\"Slider Height\" name=\"slideHeight\" min=\"300\" max=\"700\" step=\"10\" value=\"450\" condition=\"showImgText || showImgTitle || showImg\">\n        <select title=\"Sliders Count\" name=\"slidersAmount\" condition=\"showImgText || showImgTitle || showImg\">\n            <option value=\"2\">2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\" selected=\"\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\">8</option>\n        </select>\n        <input type=\"checkbox\" title=\"Show Buttons/Bullets\" name=\"showBullets\" condition=\"showImgText || showImgTitle || showImg\">\n            <input type=\"color\" title=\"Color\" name=\"buttonsColor\" value=\"#ff5e14\" condition=\"showImgText || showImgTitle || showImg\">\n        <input type=\"checkbox\" title=\"Image\" name=\"showImg\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showImgText\" checked=\"\">\n        <input type=\"checkbox\" title=\"Title\" name=\"showImgTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Line Before Title\" name=\"lineTitle\" condition=\"showImgTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Padding\" name=\"testimonialsPadding\" condition=\"(showImgText || showImgTitle) &amp;&amp; showImg\" checked=\"\">\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"testimonialsBg\" condition=\"showImgText || showImgTitle\">\n        <input type=\"color\" title=\"Color\" name=\"textBg\" value=\"#ffffff\" condition=\"!testimonialsBg &amp;&amp; (showImgText || showImgTitle)\">\n        <select title=\"Align\" name=\"textImgAlign\" condition=\"showImgTitle || showImgText\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n    <header>Text</header>    \n        <input type=\"checkbox\" title=\"Title\" name=\"showCardTitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showCardSubtitle\" checked=\"\">\n        <input type=\"checkbox\" title=\"Text\" name=\"showCardText\">\n        <input type=\"checkbox\" title=\"List\" name=\"showCardList\" checked=\"\">\n            <input type=\"range\" inline=\"\" title=\"Count\" name=\"listCount\" min=\"1\" max=\"9\" step=\"1\" value=\"1\" condition=\"showCardList\"> \n            <select title=\"Stylized Counters\" name=\"stylizedCounters\" condition=\"showCardList\">\n                <option value=\"1\">Counters</option>\n                <option value=\"2\" selected=\"\">Markers</option>\n                <option value=\"3\">Icons</option>\n            </select>\n            <input type=\"checkbox\" title=\"Border Bottom Counters\" name=\"countersBorder\" checked=\"\" condition=\"showCardList &amp;&amp; stylizedCounters == 1\">\n            <input type=\"range\" inline=\"\" title=\"Width Marker\" name=\"widthImg\" min=\"0.8\" max=\"6\" step=\"0.1\" value=\"0.8\" condition=\"showCardList\">\n            <input type=\"checkbox\" title=\"Title List\" name=\"showCardListTitle\" condition=\"showCardList\">\n            <input type=\"checkbox\" title=\"Text List\" name=\"showCardListText\" checked=\"\" condition=\"showCardList\">\n        <input type=\"checkbox\" title=\"Milestone With Number\" name=\"showMilestone\" checked=\"\">\n            <input type=\"checkbox\" title=\"Border Top\" name=\"showBorderTop\" condition=\"showMilestone\" checked=\"\">\n            <input type=\"checkbox\" title=\"Border Right\" name=\"showBorderRight\" condition=\"showMilestone &amp;&amp; milCount != 1 &amp;&amp; columnWidth != 5\" checked=\"\">\n            <input type=\"color\" title=\"Color Border\" name=\"borderColor\" value=\"#bec1c7\" condition=\"showMilestone &amp;&amp; (showBorderTop || (showBorderRight &amp;&amp; milCount != 1))\">\n            <input type=\"range\" inline=\"\" title=\"Count Milestone\" name=\"milCount\" min=\"1\" max=\"4\" step=\"1\" value=\"2\" condition=\"showMilestone\">\n        <select title=\"Align\" name=\"cardAlign\" condition=\"showCardTitle || showCardSubtitle || showCardList || showCardText || showMilestone\">\n            <option value=\"1\" selected=\"\">Left</option>\n            <option value=\"2\">Center</option>\n            <option value=\"3\">Right</option>\n        </select>\n        <input type=\"checkbox\" title=\"Padding\" name=\"showPadding\" condition=\"(showCardTitle || showCardSubtitle || showCardList || showCardText || showMilestone) &amp;&amp; (showImgText || showImgTitle || showImg)\" checked=\"\">\n        <input type=\"checkbox\" title=\"Transparent BG\" name=\"transparentBg\" condition=\"showCardTitle || showCardSubtitle || showCardText || showCardList || showMilestone\" checked=\"\">\n        <input type=\"color\" title=\"Color\" name=\"cardBg\" value=\"#bec1c7\" condition=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardText || showCardList || showMilestone)\">\n        <input type=\"range\" inline=\"\" title=\"Opacity\" name=\"cardOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"!transparentBg &amp;&amp; (showCardTitle || showCardSubtitle || showCardText || showCardList || showMilestone)\">\n    <header>Background</header>\n        <input type=\"checkbox\" title=\"2 Background Color\" name=\"twoBgColor\" condition=\"bg.type == 'color'\" checked=\"\"> \n        <input type=\"range\" inline=\"\" title=\"Width 2 Bg\" name=